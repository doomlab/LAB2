journal = {Behavior Research Methods},
number = {2},
pages = {522--530},
pmid = {18522063},
title = {{On the norming of homophones}},
volume = {40},
year = {2008}
}
@book{Aston1998,
abstract = {The handbook has three major parts. It begins with an introduction to the topic of corpus linguistics, intended to bring the substantial amount of corpusbased work already done in a variety of research areas to the non-specialist reader's attention. It also provides an outline description of the BNC itself. The bulk of the book however is concerned with the use of the SARA search program. This part consists of a series of detailed task descriptions which (it is hoped) will serve to teach the reader how to use SARA eVectively, and at the same time stimulate his or her interest in using the BNC. There are ten tasks, each of which introduces a new group of features of the software and of the corpus, of roughly increasing complexity. At the end of each task there are suggestions for further related work. The last part of the handbook gives a summary overview of the SARA program's commands and capabilities, intended for reference purposes, details of the main coding schemes used in the corpus, and a select bibliography},
author = {Aston, Guy and Burnard, Lou},
isbn = {0748610553},
issn = {08015775},
number = {September},
title = {{The BNC handbook: exploring the British National Corpus with SARA}},
year = {1998}
}
@article{Galambos1983,
abstract = {Six dimensions that are relevant to our knowledge of common activities are introduced and quantified. Twelve component actions in each of 30 activities were chosen. Two of the dimensions examined were the familiarity and frequency of performance of the activities themselves. The other four dimensions—sequence, centrality, distinctiveness, and standardness—are characteristics of the actions within the activities. There was high agreement among subjects on the norms for each dimension. Correlational analyses demonstrate the possibility of choosing actions in such a way as to compare different levels of the dimensions as factors in controlled experiments. The importance of considering these dimensions in interpretations of script or schema experiments is stressed.},
author = {Galambos, James A.},
doi = {10.3758/BF03203654},
