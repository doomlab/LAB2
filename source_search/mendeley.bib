@article{Janssen2011,
abstract = {We present a set of 150 pictures with morphologically complex English compound names. The pictures were collected from various sources and were standardized to appear as grayscale line drawings of a fixed size. All the compounds had two constituents and were primarily of the noun-noun type. Following previous studies, we collected name agreement (percentage and H), familiarity, image agreement, and visual complexity norms, as well as frequency estimates for the whole compound word and its first and second constituents. These pictures and their corresponding norms (available from the Psychonomic Society's supplemental archive) are a valuable tool in the study of the morphological representation of complex words in language processing.},
author = {Janssen, Niels and Pajtas, Petra E. and Caramazza, Alfonso},
