journal = {Behavior Research Methods},
month = {feb},
number = {1},
pages = {392--405},
publisher = {Springer US},
title = {{Lisbon Emoji and Emoticon Database (LEED): Norms for emoji and emoticons in seven evaluative dimensions}},
url = {http://link.springer.com/10.3758/s13428-017-0878-6},
volume = {50},
year = {2018}
}
