}
@article{Nodine1968,
abstract = {All 2100 CVC trigrams were scaled for pronunciability by measuring pronunciation latency (PLat). The resulting distribution of PLat scores was extremely leptokurtic and positively skewed. Scores ranged from a minimum of .531 sec to a maximum of 1.726 sec. Average PLat was .81 sec. The relationship of PLat to Archer meaningfulness was linear; however, the degree of relationship was slight (r=−.37). This finding is interpreted as indicating that PLat is relatively free of bias from such other stimulus attributes as meaningfulness. As such, P Lat is viewed as reflecting a basic processing time for such stimulus materials.},
