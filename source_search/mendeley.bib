volume = {48},
year = {2016}
}
@article{Gibson1988,
abstract = {Cognitive psychology is finding increasing use for the word fragment completion test, in which words have to be completed from a subset of their letters (e.g., horizon from --r-z--). Researchers often try to restrict their fragments to those that can be completed with only one word, but this is difficult to do and probably never has been achieved. To help resolve this problem, a list is provided of 1,086 three- to eight-letter words, each of which is uniquely specified by a two-letter fragment, where uniqueness is defined on the basis of two sizable word collections.},
author = {Gibson, Janet M. and Watkins, Michael J.},
