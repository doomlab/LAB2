journal = {Behavior Research Methods},
keywords = {Latin alphabets,Letter visual-similarity/confusability matrix,Literacy acquisition,Reading},
number = {2},
pages = {431--439},
pmid = {23055176},
title = {{A letter visual-similarity matrix for Latin-based alphabets}},
volume = {45},
year = {2013}
}
