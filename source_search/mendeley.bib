@article{Gabert-Quillen2015,
abstract = {Film clips are widely utilized to elicit emotion in a variety of research studies. Normative ratings for scenes selected for these purposes support the idea that selected clips correspond to the intended target emotion, but studies reporting normative ratings are limited. Using an ethnically diverse sample of college undergraduates, selected clips were rated for intensity, discreteness, valence, and arousal. Variables hypothesized to affect the perception of stimuli (i.e., gender, race-ethnicity, and familiarity) were also examined. Our analyses generally indicated that males reacted strongly to positively valenced film clips, whereas females reacted more strongly to negatively valenced film clips. Caucasian participants tended to react more strongly to the film clips, and we found some variation by race-ethnicity across target emotions. Finally, familiarity with the films tended to produce higher ratings for positively valenced film clips, and lower ratings for negatively valenced film clips. These findings provide normative ratings for a useful set of film clips for the study of emotion, and they underscore factors to be considered in research that utilizes scenes from film for emotion elicitation.},
author = {Gabert-Quillen, Crystal A. and Bartolini, Ellen E. and Abravanel, Benjamin T. and Sanislow, Charles A.},
doi = {10.3758/s13428-014-0500-0},
isbn = {1554-3528 (Electronic)$\backslash$r1554-351X (Linking)},
issn = {15543528},
journal = {Behavior Research Methods},
keywords = {Affect dimensions,Discrete emotions,Emotion elicitation,Emotional films,Gender differences,Validation},
number = {3},
pages = {773--787},
pmid = {24984981},
title = {{Ratings for emotion film clips}},
volume = {47},
year = {2015}
}
@article{Luke2018,
abstract = {This article presents the Provo Corpus, a corpus of eye-tracking data with accompanying predictability norms. The predictability norms for the Provo Corpus differ from those of other corpora. In addition to traditional cloze scores that estimate the predictability of the full orthographic form of each word, the Provo Corpus also includes measures of the predictability of the morpho-syntactic and semantic information for each word. This makes the Provo Corpus ideal for studying predictive processes in reading. Some analyses using these data have previously been reported elsewhere (Luke {\&} Christianson, 2016). The Provo Corpus is available for download on the Open Science Framework, at https://osf.io/sjefs .},
author = {Luke, Steven G. and Christianson, Kiel},
