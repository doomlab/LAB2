}
@article{Emmerich1979,
abstract = {Ratings of concreteness and picturability and production data for meaningfulness of 310 words were gathered from 207 6th-grade children and 265 college adults. Adults also provided ratings of imagery value. Correlations among the various stimulus attributes indicated that for adults, imagery, concreteness, and picturability were overlapping attributes. For children, however, the attributes of concreteness and picturability did not overlap as much.},
author = {Emmerich, Helen J.},
