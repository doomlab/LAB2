@inbook{2004-14277-01019830101,
Abstract = {Individual talents in reading vary widely. For example, the average college student reads about 250 words per minute, but some read at twice that rate without a loss of comprehension and many read at half that rate with no gain in comprehension. Children just beginning to read--at about age six in the United states--also vary widely in reading ability, with many children not showing adequate progress in reading words. Somewhat later, their problems show up in comprehension as well. These reading ability differences are the focus of this paper. In order to deal with these differences, the author first considers the component processes of skilled reading in a general cognitive framework. At the most general level, these components are lexical access and comprehension. Lexical access is the 'recognition' of a word. It is access to the word in permanent memory given a printed graphic input. Comprehension is not a single process, but the result of a number of processes that the reader uses to produce understanding. An important link between lexical access and comprehension may be provided by implicit speech processes that occur as part of lexical access. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Perfetti, Charles A.},
Booktitle = {Issues in cognition:  Proceedings of a joint conference in psychology.},
ISBN = {0-912704-98-5, 978-0912704982},
Pages = {137 - 161},
Publisher = {National Academy of Sciences, American Psychological Association},
Title = {Individual differences in reading abilities},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2004-14277-010&site=ehost-live&scope=site},
Year = {1983},
}


@book{1987-97426-00019760101,
Abstract = {This book attempts to lay the foundation for a new field of language study, a field that we sometimes hear ourselves calling psycholexicology, the psychological study of the lexical component of language. (PsycINFO Database Record (c) 2016 APA, all rights reserved), Our own goal is to achieve a theoretical synthesis of the external and the internal: to deal with problems of reference without ignoring the complexities of lexical memory, and also to deal with problems of meaning without ignoring the complexities of language use. Given this goal, our expository strategy develops as follows. First we will explore the inadequacies of any psycholexicology based solely on perception. In the course of that exploration we hope both to gain a new perspective toward perceptual processes by viewing them in terms of their contribution to human language and also to collect some ideas about the kind of externally related system that is needed to buttress current psychological theories of lexical mem)},
Author = {Miller, George A. and  Johnson-Laird, Philip N.},
