Keywords = {Specific language impairment, Sentence repetition, Sensibility, Specificity, Construct validity, Case-Control Studies, Child, Female, France, Humans, Language Development Disorders, Language Tests, Male, Principal Component Analysis, Reproducibility of Results, Sensitivity and Specificity, Diagnosis, Psychometrics, Sentences, Test Reliability, Specific Language Impairment, Construct Validity, Semantics, Test Validity},
Number = {12},
Pages = {3423 - 3430},
Title = {The sentence repetition task: A powerful diagnostic tool for French children with specific language impairment},
URL = {al.leclercq@ulg.ac.be, ORCID: 0000-0002-7091-8293},
Volume = {35},
Year = {2014},
}


@article{2014-48803-00720141201,
Abstract = {This study investigated the role of morphological and contextual information in inferring the meaning of unknown L2 words during reading. Four groups of college‐level ESL students, beginning (n = 34), intermediate (n = 27), high‐intermediate (n = 21), and advanced (n = 25), chose the inferred meanings of 20 pseudo compounds (e.g., rainfime) from multiple choice options. The pseudo compounds were presented in a set of sentences. In the Morphology Reliable condition, the known word part in the pseudo compounds provided semantic information about the meaning of the compounds, which also matched the context of the sentences. In the Morphology Unreliable condition, the known word part did not provide any reliable semantic information nor did it match the context. The critical answer options were the meanings based on morphological information and the meanings based on contextual information. Results indicated significant differences in the choice of answers between the groups only in the Morphology Unreliable condition: The beginning‐level learners relied more on morphological information even though the meanings based on morphology were incorrect. The study discusses the underlying processes in the choice of information used in lexical inference during reading. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
