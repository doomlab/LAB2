Year = {1988},
}


@article{1989-14355-00119880101,
Abstract = {Manipulated visual hemifield, stimulus duration, and angle of eccentricity using a lexical decision paradigm with 6 adult Ss. Lexical decision was measured by several dependent variables: reaction time (RT), accuracy, d′-1; and β. Results support the generalization of right visual field superiority in a lexical decision task for parafoveal stimulation for 3 dependent variables, RT, accuracy, and d′ 1. When RT is plotted as a function of accuracy, data separate into 2 groups, each best described by a linear equation with negative slope. It is hypothesized that the same basic process discriminates words from nonwords in a lexical decision task. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
