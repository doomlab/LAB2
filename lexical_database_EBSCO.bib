}


@inbook{2012-23040-00520120101,
Abstract = {In this chapter, we review a relatively recent approach to studying lexical processing, which involves developing large databases that are made available for researchers to study across three distinct domains; isolated visual word recognition, semantic priming, and recognition memory. One of the goals of this research endeavor is to help define the common set of principles that researchers can rely upon in better understanding how lexical processing influences critical aspects of cognition. This megastudy approach contrasts with the more traditional approach of factorial experiments targeting specific variables within small-scale studies. We will ultimately argue that progress in this field is going to depend on a judicious combination of targeted factorial studies and large scale databases. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
