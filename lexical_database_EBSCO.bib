Title = {Lexical creativity during instances of word-finding difficulty: Broca's vs Wernicke's aphasia},
Volume = {20},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=1984-04378-001&site=ehost-live&scope=site},
