@article{1998-12962-00319980801,
Abstract = {A controversial phenomenon concerning slips of the tongue is the tendency for the onsets of words to slip more frequently than segments at other word positions. Some researchers attribute this effect to the phonological properties of word onsets, while others suggest it reflects something specific about the role played by the word onset during phonological recovery and/or sequencing. This study examined these two possibilities using a tongue-twister paradigm in which 10 Ss were asked to repeatedly recite a visually-presented word quadruple. The lexical status of the words was manipulated independently of their broader phonological characteristics by altering their constituent vowels. Errors elicited on real word targets were found to exhibit a strong word onset effect; this effect was entirely confined to between—word contextual errors. However, nonword targets generated no word onset effect, either overall or on between—word errors considered alone. These findings suggest that the word onset effect cannot be entirely attributed to phonological factors, but instead reflects the larger role word onsets play in speech planning. A new account is offered, which attributes the word onset effect to order-based competition between items at the lexical level. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Wilshire, Carolyn E.},
ISSN = {0010-0277, 1873-7838},
Journal = {Cognition},
Keywords = {serial order & word onset effect, phonological encoding & misspoken words in tongue twisters, native speakers of British English, Adult, Aged, Humans, Language, Middle Aged, Phonetics, Psycholinguistics, Speech, Errors, Language, Phonology, Psycholinguistics, Sentence Structure, Human Information Storage},
Number = {2},
Pages = {143 - 166},
Title = {Serial order in phonological encoding: An exploration of the 'word onset effect' using laboratory-induced errors},
Volume = {68},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=1998-12962-003&site=ehost-live&scope=site},
Year = {1998},
}


@article{1998-04421-00419980601,
Abstract = {Examined acquired dysgraphia observed in a 75-yr-old French-speaking woman. The results point to an impairment of the graphemic buffer, i.e., the processing stage where abstract orthographic representations are temporarily stored while planning the written production. However, the spelling errors were more frequent in the irregular than in the regular words, A qualitative analysis of the errors in the irregular misspelled words showed that these were not 'regularization' errors, but rather the same characteristics as the phonologically implausible errors found in the regular words, such as letters substitutions, deletions, additions, and transpositions. Furthermore. in a list of regular and irregular words of same length and graphemic structure, the errors not only tended to concentrate on the irregularity itself but also tended to be more frequent elsewhere in the irregular words compared to the regular words. The findings are discussed in terms of a post-lexical sensitivity to irregular spelling. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
