}


@article{2013-27662-00120130901,
Abstract = {This study investigated the processes underlying the effect of masked syllable priming in French with pseudoword primes and word targets. Two lexical-decision task (LDT) experiments examined whether the syllable priming effect depends on syllable frequency and might rely on a general abstract structure. The results of Experiment 1 revealed an inhibitory priming effect, with pseudoword primes and word targets sharing a high-frequency first syllable, which was not due to the abstract syllable structure. In contrast, no inhibition was observed with a low-frequency first syllable in Experiment 2. Syllable frequency appears to be an important factor determining the speed of target processing in masked priming. This is attributed to variations in the respective contributions of sublexical activation and lexical inhibition processes. (PsycINFO Database Record (c) 2016 APA, all rights reserved), Cette étude se penche sur les mécanismes sous-jacents à l’effet de l’amorçage masqué au moyen de s)},
Author = {Mathey, Stéphanie and  Doignon-Camus, Nadège and  Chetail, Fabienne},
