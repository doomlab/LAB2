@article{1985-16827-00119830401,
Abstract = {Investigated the hypothesis that, for adult native speakers, increasing syntactic complexity would lead to increased salience of phonological properties of words; whether syntactic simplicity would lead to a greater salience of semantic properties of words was also studied. 72 adult native speakers of English were required to name a word presented after a context sentence. Syntactic complexity of the context sentence was varied; the word to be named was phonologically or semantically related or unrelated to the last word in the sentence. The time interval between the end of the context sentence and the word to be named was also varied (0, 50, or 200 msec). Data provide no support for the hypothesis that complex syntax leads to increased salience of phonological properties of words, as there was no substantial or consistent increase in RTs in the complex as opposed to the easy syntax conditions. Data suggest, however, that there is some interplay between lexical accessing and syntactic processing: There was a tendency for the frequency of the word to be named to correlate with naming time in the complex conditions (i.e., less frequent words were named more slowly). It was also found that salience of activated words was maintained under complexity conditions as the time for sentence processing was increased. It is suggested that these trends can be integrated into a processing model that contains autonomous subunits for syntactic processing and lexical access. (12 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Goodluck, Helen},
Journal = {Wisconsin Center for Education Research: Program Report},
Keywords = {syntactic complexity, salience of phonological word properties in naming, adult native speakers of English, Stimulus Complexity, Stimulus Salience, Syntax, Words (Phonetic Units)},
Pages = {29 - 29},
Title = {Salience of word properties in naming: Effects and non-effects of syntactic complexity},
Volume = {Rpt  83-11},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=1985-16827-001&site=ehost-live&scope=site},
Year = {1983},
}


