Keywords = {consonants, vowels, lexical access, reading, language acquisition, adult speech processing, Adult, Analysis of Variance, Cognition, Cues, Decision Making, Humans, Language, Paris, Perceptual Masking, Phonetics, Reaction Time, Reading, Reproducibility of Results, Students, Task Performance and Analysis, Visual Perception, Consonants, Language Development, Lexical Access, Reading, Vowels, Lexical Decision, Oral Communication},
Number = {12},
Pages = {1223 - 1227},
Title = {Differential processing of consonants and vowels in lexical access through reading},
URL = {boris.new@parisdescartes.fr},
