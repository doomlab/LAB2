Number = {4},
Pages = {1157 - 1173},
Title = {Associative priming in a masked perceptual identification task: Evidence for automatic processes},
URL = {pecher@fsw.eur.nl},
Volume = {55A},
Year = {2002},
}


@article{2002-06035-00120021001,
Abstract = {This study (n = 123 undergraduate students) explores the sensitivity of a new paradigm-- pause detection-- to lexical activity during spoken word processing. Short pauses inserted in five-syllable-long spoken sequences were detected more slowly after words than after spliced nonwords. This effect was substantial for one- and two-syllable initial sequences, and gradually decreased with longer stimuli. Lexical inhibition was also found to depend on the uniqueness point of the stimuli: Late-unique stimuli caused pause detection to be delayed. Finally, pauses were detected more slowly after high-activity than low-activity nonwords (based on the size of their initial cohort). It is argued that pause detection latencies can be used as a gauge of lexical activity in speech sequences and, in particular, at word boundaries. The value of the pause detection paradigm is discussed by reference to other paradigms and their ability to tap, on-line, lexical activation and decay. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Mattys, Sven L. and  Clark, Jamie H.},
ISSN = {0749-596X, 1096-0821},
Journal = {Journal of Memory and Language},
Keywords = {lexical activity, speech processing, pause detection, Cognitive Processes, Lexical Decision, Speech Pauses, Words (Phonetic Units)},
Number = {3},
Pages = {343 - 359},
Title = {Lexical activity in speech processing: Evidence from pause detection},
URL = {sven.mattys@bris.ac.uk},
Volume = {47},
Year = {2002},
}


@article{2002-04315-00220021001,
Abstract = {Notes that an important determinant of picture and word naming speed is the age at which the names were learned (age of acquisition). Two related interpretations of these effects are that they reflect differences between words in their cumulative frequency of use, or that they reflect differences in the amount of time early and late-acquired words have spent in lexical memory. Both theories predict that differences between early and late-acquired words will be less apparent in older than younger adults. Two experiments are reported in which younger and older adults read words varying in age of acquisition or frequency, or named objects varying in age of acquisition. Experiment 1 involved 28 subjects aged 18-25 yrs and 32 subjects aged 63-86 yrs. Experiment 2 involved 35 subjects aged 18-32 yrs, 32 subjects aged 60-69 yrs, and 32 subjects aged 80-93 yrs. There was an observed effect of word frequency only for young adults' word naming. In contrast, strong age of acquisition effects were found for both the young and the old participants. The implications of these results for theories of how age of acquisition might affect lexical processing are discussed. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Morrison, Catriona M. and  Hirsh, Katherine W. and  Chappell, Tameron and  Ellis, Andrew W.},
ISSN = {0954-1446, 1464-0635},
Journal = {European Journal of Cognitive Psychology},
Keywords = {age differences, age of acquisition, word learning, naming speed, word naming, object naming, word frequency, lexical memory, Age Differences, Language Development, Lexical Access, Naming, Word Frequency, Cognitive Processing Speed, Words (Phonetic Units)},
Number = {4},
Pages = {435 - 459},
Title = {Age and age of acquisition: An evaluation of the cumulative frequency hypothesis},
URL = {Morrison@cardiff.ac.uk},
Volume = {14},
Year = {2002},
}


@article{2002-04315-00120021001,
Abstract = {The effect of morphological complexity on word identification was studied in 3 experiments conducted in Finnish, employing the same set of target nouns. In Experiment 1 (n=18), the target nouns were presented in isolation, and lexical decision times were employed as lexical access measures. In Experiments 2 and 3, the same words were embedded in sentence contexts, where both the inflected and non-inflected forms were equally plausible, and eye fixation patterns (Experiment 2; n=24) and lexical decision latencies (Experiment 3; n=21) were recorded. The experiment with isolated words replicated previous lexical decision studies by showing more effortful processing for inflected than monomorphemic nouns. However, this morphological complexity effect did not generalise to the context experiments; fixation durations and response latencies were highly similar for inflected and monomorphemic words. It is suggested that, at least for the type of inflected nouns studied, the morphological effect observed for isolated words may derive from the syntactic and/or semantic level and not necessarily from the lexical level, as previously assumed. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Hyönä, Jukka and  Vainio, Seppo and  Laine, Matti},
ISSN = {0954-1446, 1464-0635},
Journal = {European Journal of Cognitive Psychology},
Keywords = {word identification, morphological complexity, nouns, isolated words, lexical decision times, lexical decision latencies, sentence context, eye fixation patterns, Contextual Associations, Lexical Decision, Morphology (Language), Sentences, Words (Phonetic Units), Eye Fixation, Nouns, Response Latency},
Number = {4},
Pages = {417 - 433},
Title = {A morphological effect obtains for isolated words but not for words in sentence context},
URL = {hyona@utu.fi},
Volume = {14},
Year = {2002},
}


@article{2002-04252-00420021001,
Abstract = {To explore whether imitation of novel behaviors might serve as a lexical acquisition strategy for some infants, relations between infants' early spontaneous reproduction of novel and of familiar words and their subsequent lexicons were compared in a longitudinal sample of 20 infants during natural interactions with their mothers at 13, 17, and 21 mo old. Both maternal report and observational measures of noun and non-noun lexicons were analyzed. There were marked contrasts in quantity and proportions of nouns between reported and observed lexicons. However, when earlier vocabulary levels were statistically controlled, infants' early replication of novel, but not familiar, words was associated with growth in both reported and observed noun and non-noun vocabularies. Infants' early imitation of novel words predicts, and may facilitate, their later lexical development. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Masur, Elise Frank and  Eichorst, Doreen L.},
ISSN = {0272-930X, 1535-0266},
