Year = {2001},
}


@article{2001-01980-01120010701,
Abstract = {Compared the eye movements of 2 patients who read letter-by-letter (LBL) following a left occipital lobe lesion with those of 7 normal controls and 4 hemianopic Ss in a nonreading visual search task and a text reading task. While the LBL readers exhibited similar eye-movement patterns to those of the other 2 groups on the nonreading task, their eye movements differed significantly during reading, as reflected in the disproportionate increase in the number and duration of fixations per word and in the regressive saccades per word. Importantly, relative to the 2 control groups, LBL readers also made more fixations per word as word length increased, especially as word frequency and word imageability decreased. The alteration in the oculomotor behavior of the LBL readers during reading is similar to that seen in normal readers under difficult reading conditions, as well as in beginning readers and in those with developmental dyslexia, and appears to reflect difficulties in processing the visual stimulus. The interaction of length with frequency and with imageability in determining the eye movement pattern is consistent with an interactive activation model of normal word recognition in which weakened activation of orthographic input can nevertheless engage high-level lexical factors. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
Author = {Behrmann, M. and  Shomstein, S. S. and  Black, S. E. and  Barton, J. J. S.},
