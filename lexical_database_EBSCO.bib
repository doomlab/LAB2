URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=1979-29610-001&site=ehost-live&scope=site},
Year = {1979},
}


@article{1979-27819-00119790701,
Abstract = {A variety of experimental findings have indicated that a system of precategorical acoustic storage is responsible for the recency effect obtained in the immediate serial recall of sequences of digits, consonants, or syllables. This study investigated whether such findings could be generalized to the recall of sequences of words by 16 undergraduates. Exp I showed that phonemic similarity among a sequence of words failed to reduce the modality effect or the recency effect. Exp II demonstrated that this finding was not attributable to a failure to control the phonemic properties of the stimulus material. Exp III showed that the stimulus suffix effect obtained with sequences of words was not affected by the acoustic similarity between the list items and the stimulus suffix. Exp IV demonstrated that phonemic similarity among a sequence of words failed to reduce the stimulus suffix effect. Results are explained by extending the original model of short-term memory to incorporate a system of postcategorical lexical storage. (32 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Richardson, John T.},
ISSN = {0010-0285},
Journal = {Cognitive Psychology},
Keywords = {word sequences & modality & phonemic similarity, precategorical acoustic storage & postcategorical lexical storage, college students, Auditory Perception, Memory, Phonemes, Recall (Learning), Words (Phonetic Units), Human Information Storage, Recency Effect, Visual Perception, Word Meaning},
Number = {3},
Pages = {265 - 286},
Title = {Precategorical acoustic storage and postcategorical lexical storage},
