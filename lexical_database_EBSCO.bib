Title = {Mechanisms of phonological inference in speech perception},
URL = {ORCID: 0000-0003-0690-6308},
Volume = {24},
Year = {1998},
}


@article{1998-02351-00619980301,
Abstract = {Examined whether differences among 1st languages (L1) affect word recognition in reading a 2nd language (L2). 16 participants had Indonesian (an alphabetic language) and 16 had Chinese (a logographic language) as L1 and were learning English (an alphabetic language) as L2. Ss engaged in a lexical decision task using English words. Under the connectionist rubric, it was predicted that an alphabetic L1 would facilitate word recognition in an alphabetic L2, especially if the L1 and L2 have similar spelling patterns.  Results support the connectionist word recognition model. The better performance of the Indonesian Ss, relative to the Chinese Ss, suggests that the Indonesian readers developed a relatively strong interletter associative network through their L1 experience, which they transferred to the lexical decision task.  In contrast, the Chinese readers, who were accustomed to processing primarily logographic characters in their L1, did not have the advantage of transferring such L1 knowledge. The results substantiate the hypothesis that an L1 interletter associative network would facilitate L2 word recognition when L1 and L2 have similar orthographic systems. Materials used in the lexical decision task are appended. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Muljani, D. and  Koda, Keiko and  Moates, Danny R.},
ISSN = {0142-7164, 1469-1817},
Journal = {Applied Psycholinguistics},
