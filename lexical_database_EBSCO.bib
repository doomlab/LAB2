Title = {Syntactic priming in young children},
URL = {hutt@uchicago.edu},
Volume = {50},
Year = {2004},
}


@article{2004-11174-01720040201,
Abstract = {We investigated event-related potential (ERP) correlates of developmental changes in spoken word recognition during early school years. We focused on implicit processing of word onsets as this may change considerably due to vocabulary growth and reading acquisition. Subjects were pre-schoolers, beginning readers and adults. Two experiments examined phonological onset priming effects on ERP measures in an auditory lexical decision task. Primes were words (Expt. 1) or non-words (Expt. 2). ERPs elicited by words showed clear developmental changes in ERP latency and morphology, especially with regard to early negativities like the N1. Prominent priming effects were an enhanced N400 amplitude due to alliterating word primes in beginning readers, and typical phonological N400 reductions due to alliterating non-word primes in all groups. Priming further led to opposite early effects with word primes vs. non-word primes in all groups. In particular, our results indicate distinct processing of word onsets in beginning readers, confirming that vocabulary growth and the acquisition of reading may critically contribute to the formation of a fully segmental lexical system. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Bonte, Milene and  Blomert, Leo},
ISSN = {1388-2457},
