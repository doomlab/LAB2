Number = {6},
Pages = {1177 - 1182},
Title = {The locus of the frequency effect in picture naming: When recognizing is not enough},
URL = {jalmeida@wjh.harvard.edu},
Volume = {14},
Year = {2007},
}


@article{2007-19981-02220071201,
Abstract = {Recent data from compound word processing suggests that compounds are recognized via their constituent lexemes (Juhasz, Starr, Inhoff, & Placke, 2003). The present lexical decision experiment manipulated orthogonally the frequency of the constituents of compound words in two languages: Basque and Spanish. Basque and Spanish diverge widely in their morphological properties and in the number of existing compound words. Furthermore, the head lexeme (i.e., the most meaningful lexeme related to the whole-word meaning) in Spanish tends to be the second lexeme, whereas in Basque the percentage is more distributed. Results showed a facilitative effect of the frequency of the second lexeme, in both Basque and Spanish compounds. Thus, both Basque and Spanish readers decompose compounds into their constituents for lexical access, and this decomposition is carried out in a language-independent and blind-to-semantics manner. We examine the implications of these results for models of lexical access. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
Author = {Duñabeitia, Jon Andoni and  Perea, Manuel and  Carreiras, Manuel},
ISSN = {1069-9384, 1531-5320},
Journal = {Psychonomic Bulletin & Review},
Keywords = {lexical access, decision making, word frequency, compound words, Spanish, Basque, morphology, Humans, Language, Spain, Vocabulary, Language, Lexical Decision, Morphology (Language), Word Frequency, Words (Phonetic Units)},
Number = {6},
Pages = {1171 - 1176},
Title = {The role of the frequency of constituents in compound words: Evidence from Basque and Spanish},
URL = {jaduna@ull.es, ORCID: 0000-0002-3312-8559},
Volume = {14},
Year = {2007},
}


@article{2007-18737-00220071201,
Abstract = {The ability of school children (N = 233) to acquire new scientific vocabulary was examined. Children from two age groups (M = 4.8 and M = 6.5) were introduced to previously unknown words in an educational video. Word knowledge was assessed through accuracy and latency for production and comprehension over a 9-month period. A draw and write task assessed acquisition of domain knowledge. Word learning was poorer than has previously been reported in the literature, and subject to influences of word type (domain specificity) and word class. The results indicate that the acquisition of scientific terms is a complex process moderated by lexical, semantic and pragmatic factors. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
