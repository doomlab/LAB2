Volume = {31},
Year = {2003},
}


@article{2003-07035-00320030701,
Abstract = {Examined sentence context and grammatical gender on lexical access using a timed word-naming paradigm. Ss were 25 male and female native Italian speakers (aged 19-27 yrs). A total of 550 two-sentence contexts were prepared (440 for use within the factorial gender-by-semantics design; 110 neutral controls containing no semantic or gender constraints). 110 masculine/feminine word targets were used. Large main effects of both sentence context and the gender of the article were observed; the interaction between gender and semantics was significant over Ss. Strong facilitation by both gender and semantics was observed, relative to a neutral-control baseline condition. Results are compared with a prior study with the same design, using a picture-naming paradigm; a separate norming study of timed word reading in a list format, using the same stimuli; and a prior study of German comparing word and picture naming in short, semantically neutral phrases. Differences in methodology and in findings between the Italian word naming and the German word naming are compared and discussed. Findings of the present study are interpreted in support of interactive-activation models in which different sources of information are combined on-line to predict, anticipate, or preactivate lexical targets. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
