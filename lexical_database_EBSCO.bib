Year = {2014},
}


@article{2014-14646-00620140501,
Abstract = {Despite grammatical class being a fundamental organising principle of the human mental lexicon, recent morphological models of visual word identification remain silent as to whether and how it is represented in the lexical system. The present study addresses this issue by investigating cross-class morphological priming (i.e., the effect obtained when nouns prime verbs sharing the same root or vice versa) to clarify whether morphological stems subserving the formation of both nouns and verbs (e.g., depart-) have a unique, grammatical class–independent representation. Experiments 1 and 2 suggest this to be the case, as they show that morphological priming crosses grammatical class boundaries in overt paradigm conditions. Experiment 3 shows that, in masked priming conditions, cross-class facilitation emerges both for genuine derivations and pseudo-related pairs with a homographic stem (e.g., port-e, doors, and port-are, to carry), which is taken to suggest that grammatical class–free stem representations are located at a pre-lexical level of morphological processing. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Crepaldi, Davide and  Morone, Elena Angela and  Arduino, Lisa Saskia and  Luzzatti, Claudio},
ISSN = {2044-5911, 2044-592X},
Journal = {Journal of Cognitive Psychology},
Keywords = {morphological processing, nouns, verbs, visual word identification, lexical system, priming, Morphology, Nouns, Priming, Verbs, Word Recognition, Mental Lexicon},
Number = {4},
Pages = {433 - 460},
Title = {Morphological processing of printed nouns and verbs: Cross-class priming effects},
URL = {davide.crepaldi1@unimib.it, ORCID: 0000-0002-6765-1516},
Volume = {26},
Year = {2014},
}


@article{2014-02533-00120140501,
Abstract = {The majority of Chinese characters are composed of a semantic and a phonetic radical. Although pre-lexical involvement of the phonetic radical in Chinese character recognition has been established, there is little agreement on the contribution of the semantic radical. We used magnetoencephalography (MEG) to investigate the temporal dynamics of the priming effect elicited by repetition of phonetic and semantic radicals in the tasks of homophone and synonym judgment, respectively. The behavioral results revealed robust priming induced by both kinds of radicals. As for the MEG results, the repetition effect of phonetic radicals was obtained in the M170, M250 and M350 components. However, the repetition effect of semantic radicals was only marginally interactive with other variables in the M170, M350 and M450 components. The present findings suggest that phonetic radicals play a predominant role in early lexical access and phonological computation of Chinese character recognition,while semantic radicals have relatively weak effect on lexical access and semantic retrieval. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Hung, Yi-hui and  Hung, Daisy L. and  Tzeng, Ovid J.-L. and  Wu, Denise H.},
ISSN = {0911-6044},
Journal = {Journal of Neurolinguistics},
Keywords = {phonetic radicals, semantic radicals, synonym judgment, priming paradigm, MEG recording, homophone judgments, synonym judgments, orthographic priming, radical repetition effect, Orthography, Phonetics, Semantic Priming, Synonyms, Word Recognition, Magnetoencephalography, Priming, Semantics, Tracking},
Pages = {42 - 65},
Title = {Tracking the temporal dynamics of the processing of phonetic and semantic radicals in Chinese character recognition by MEG},
URL = {yihuihung1116@gmail.com, dhung@ncu.edu.tw, ovid@gate.sinica.edu.tw, wuhsien@gmail.com},
Volume = {29},
