Year = {1993},
}


@article{1994-16215-00119931201,
Abstract = {Semantic context effects were investigated during on-line discourse processing in 48 undergraduates. Readers' eye movements were registered to see whether words that were semantically closely related to the global theme of the text were read faster than words that did not have any apparent semantic link to the discourse theme. In addition, lexical priming was examined by presenting an identity prime earlier in the text. Results show that nonthematic words were regressed to more often than thematic words. Regressions were typically initiated after reaching a clause or sentence boundary. Regressions were thus assumed to be made to integrate nonthematic words into the previous context. Modest negative correlations were found between a word's fixation time and its relative predictability. No effects of lexical priming were observed. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
