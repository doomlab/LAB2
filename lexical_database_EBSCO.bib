}


@article{2003-10088-03520031101,
Abstract = {Current models of word production assume the existence of functionally distinct lexical-syntactic and word-form representations. However, there are competing hypotheses about whether prior access to syntactic properties of words is obligatory during lexical retrieval. In this paper we report preliminary evidence from the single case study of a Welsh-speaking patient with a lexical access deficit. We examined the patient's ability to access word forms and to retrieve information about the grammatical gender of Welsh nouns. The data show no correlation between the retrieval of word-form and grammatical gender information. This pattern of performance is inconsistent with hypotheses postulating obligatory syntactic mediation in lexical production. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
