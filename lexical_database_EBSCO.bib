Keywords = {deaf signers, sign language, phonology, semantics, human development, lexical access, tip of fingers phenomenon, Adult, Deafness, Female, Humans, Language, Male, Verbal Behavior, Deaf, Lexical Access, Phonology, Sign Language, Human Development, Semantics},
Number = {11},
