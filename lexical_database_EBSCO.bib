@article{1986-05669-00119850901,
Abstract = {Hemisphere differences in lexical processing may be due to asymmetry in the organization of lexical information, in procedures used to access the lexicon, or both. Six lateralized, lexical-decision experiments with 30 right-handed undergraduates employed various types of priming to distinguish among these possibilities. In 3 controlled (high probability) priming experiments, prime words could be used as lexical access clues. Larger priming was obtained for orthographically similar stimuli (e.g., beak–bear) when presented to the left visual field (LVF). Controlled priming based on phonological relatedness (e.g., juice–moose) was equally effective in either visual field. Semantic similarity (e.g., inch–yard) produced larger priming for right visual field (RVF) stimuli. Results suggest that the hemispheres may utilize different information to achieve lexical access. Spread of activation through the lexicon was measured in 3 complementary, automatic (low probability) priming experiments. Priming was restricted to LVF stimuli for orthographically similar words, while priming for phonologically related stimuli was only obtained in the RVF. Automatic semantic priming was present bilaterally but was larger in the LVF. Results imply hemisphere differences in lexical organization, with orthographic and semantic relationships available to the right hemisphere and phonological and semantic relations available to the left hemisphere. Findings support the notion of hemisphere asymmetries in both lexical organization and directed lexical processing. (50 ref) (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Chiarello, Christine},
ISSN = {0093-934X, 1090-2155},
Journal = {Brain and Language},
Keywords = {automatic vs controlled priming for orthographically vs phonologically vs semantically similar stimuli presented to left vs right visual field, lexical organization & access, right handed college students, Adolescent, Adult, Dominance, Cerebral, Female, Humans, Male, Models, Psychological, Phonetics, Reaction Time, Reading, Semantics, Syndrome, Visual Fields, Visual Perception, Cerebral Dominance, Cognitive Discrimination, Cues, Human Information Storage, Words (Phonetic Units), Orthography, Phonology, Semantics, Word Associations},
Number = {1},
Pages = {146 - 172},
Title = {Hemisphere dynamics in lexical access: Automatic and controlled priming},
