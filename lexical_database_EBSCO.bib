Pages = {125 - 137},
Title = {Language networks associated with computerized semantic indices},
URL = {pakh0002@umn.edu},
Volume = {104},
Year = {2015},
}


@article{2015-03493-00920150101,
Abstract = {The goal of this study was to investigate the effect of Chinese‐specific morphological awareness on vocabulary acquisition among young Chinese‐speaking students. The participants were 288 Chinese‐speaking second graders from three different cities in China. Multiple regression analysis and mediation analysis were used to uncover the mediated and unmediated effects of morphological awareness on reading vocabulary knowledge. Results showed that both morphological awareness and lexical inference ability significantly contributed to reading vocabulary knowledge after each factor was controlled for. The ability to extract sublexical morphological information (in the form of derivational awareness and compound awareness) significantly predicted the students' reading vocabulary. More importantly, the study revealed that morphological awareness made a significant indirect contribution to reading vocabulary via lexical inference ability. Findings suggest that morphological awareness enhances young Chinese students' reading vocabulary acquisition by the partial mediation of their lexical inference ability. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Zhang, Haomin (Stanley)},
ISSN = {0034-0553, 1936-2722},
Journal = {Reading Research Quarterly},
Keywords = {morphological awareness, vocabulary acquisition, Chinese-speaking children, mediation, lexical inference ability, students, Ability, Lexical Access, Mediation, Morphology (Language), Vocabulary, Awareness, Inference, Morphology},
Number = {1},
Pages = {129 - 142},
Title = {Morphological awareness in vocabulary acquisition among Chinese children: Testing partial mediation via lexical inference ability},
URL = {haominzh@cmu.edu},
Volume = {50},
Year = {2015},
}


@article{2015-01029-00720150101,
Abstract = {Orthographies encode phonological information only at the level of words (chiefly, the information encoded concerns phonetic segments; in some cases, tonal information or default stress may be encoded). Of primary interest to second language (L2) learners is whether orthography can assist in clarifying L2 phonological distinctions that are particularly difficult to perceive (e.g., where one native-language phonemic category captures two L2 categories). A review of spoken-word recognition evidence suggests that orthographic information can install knowledge of such a distinction in lexical representations but that this does not affect learners’ ability to perceive the phonemic distinction in speech. Words containing the difficult phonemes become even harder for L2 listeners to recognize, because perception maps less accurately to lexical content. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Cutler, Anne},
ISSN = {0142-7164, 1469-1817},
Journal = {Applied Psycholinguistics},
Keywords = {second language phonology, lexical content, language development, orthography, spoken word recognition, Language Development, Orthography, Phonetics, Foreign Language Learning, Foreign Languages, Phonology, Word Recognition},
Number = {1},
Pages = {115 - 128},
Title = {Representation of second language phonology},
