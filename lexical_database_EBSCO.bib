Volume = {11},
Year = {2001},
}


@inbook{2004-12822-04720010101,
Abstract = {The purpose of this study was to determine if the frequency or neighborhood density of words presented to children in the input would influence the gradual emergence of sounds through lexical diffusion. Longitudinal production data were obtained from eight children, ages 3;10 to 5;4. All of the children presented with functional phonological delays, as characterized by a severely reduced consonantal inventory in the absence of other associated linguistic, motor, cognitive, or social delays. This study provides partial evidence that input is a factor governing lexical diffusion in phonological acquisition. A word's frequency is particularly predictive of which words will undergo lexical diffusion and which will not. Input alone, however, cannot account for the full range of sound change observed in acquisition. Consequently, a conjunction of input, functional, featural, and contextual variables must be examined in tandem for a cohesive view of lexical diffusion in phonological acquisition. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Morrisette, Michele L. and  Gierut, Judith A. and  Do, Anna H.-J. and  Domínguez, Laura and  Johansen, Aimee},
