Title = {Equal homophonic priming with words and pseudohomophones},
URL = {lukatela@uconnvm.uconn.edu},
Volume = {28},
Year = {2002},
}


@article{2002-02300-00920020201,
Abstract = {Examined the effect of neighborhood size on the recognition of Japanese katakana words. Neighbors of a given katakana word were defined in 2 ways: a kana-level neighbor is a katakana word that can be constructed by changing 1 letter of the target word preserving the other letter positions, and a phoneme-level neighbor is a katakana word that can be created by changing 1 phoneme preserving the other phoneme positions of the target. Ss in Exp 1 were 9 male and 21 female undergraduates and graduates (mean age 20.8 yrs) and in Exp 2 were 11 male and 19 female undergraduates and graduates (mean age 20.5 yrs), with normal vision in Japan. In Exp 1, Ss participated in a lexical decision task and numbers of kana-level neighbor were manipulated. The result showed that the number of kana-level neighbors had an inhibitory effect on the lexical decision of low-familiarity katakana words. In Exp 2, Ss participated in a lexical decision task and numbers of phoneme-level neighbors were manipulated. The result showed that the number of phoneme-level neighbors had no effect on the lexical decision of katakana words. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Kawakami, Masahiro},
ISSN = {0021-5236, 1884-1082},
Journal = {Japanese Journal of Psychology},
Keywords = {neighborhood size, kana-level neighbors, phoneme-level neighbors, lexical decision task, katakana words, Adult, Female, Humans, Language, Male, Lexical Decision, Words (Phonetic Units)},
Number = {6},
Pages = {528 - 534},
Title = {Effects of kana-level neighbors and phoneme-level neighbors on lexical decision of Japanese katakana words},
Volume = {72},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2002-02300-009&site=ehost-live&scope=site},
Year = {2002},
}


@article{2003-99292-00720020101,
Abstract = {A number of studies demonstrate the contribution of the right hemisphere (RH) in the processing of language. However, we do have not sufficient records on the perturbations of speech for such type of lesions in different languages. For this reason we studied multiple levels of linguistic and communicative performance in patients speaking Spanish in Rio de la Plata region. In this paper we present the results obtained in lexical processing tasks applied to 14 neurologically evaluated patients: mean age 67.5 years; instruction level, 9 years old, with ischemic lesion in RH. Lexical performance in nomination, recognition and lexical evocation tasks was also evaluated and its results compared with a control group of 20 healthy subjects. The study showed significant differences (< 0.05) concerning lexical semantic and phonologic evocation in 9 patients. We did not found differences in the performance of group of patients in the tasks of naming and recognition when they were compared with the control group. The deficit in patients was correlated with frontal-temporal lesions. The RH role was confirmed in the lexical semantic processing. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
