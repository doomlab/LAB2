Keywords = {imitation, lexical acquisition, infants, novel vs familiar words, nouns, Familiarity, Imitation (Learning), Language Development, Stimulus Novelty, Words (Phonetic Units), Nouns},
Number = {4},
Pages = {405 - 426},
Title = {Infants' spontaneous imitation of novel versus familiar words: Relations to observational and maternal report measures of their lexicons},
