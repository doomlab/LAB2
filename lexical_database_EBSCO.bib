Number = {2},
Pages = {67 - 88},
Title = {Promoting awareness of sounds in speech (PASS): The effects of intervention and stimulus characteristics on the blending performance of preschool children with communication impairments},
URL = {froth@hesp.umd.edu},
Volume = {29},
Year = {2006},
}


@article{2006-07839-00220060301,
Abstract = {To evaluate access to the cognitive representation of the meaning of words, in the present study we used the lexical decision paradigm developed by Marcos. In this situation, the subject has to recognize if the presented stimulus corresponds to a word or a pseudo-word with the purpose of building a model of normal processing. Once such a model of normal processing is obtained, the findings can be contrasted with pathologies in which semantic memory is altered. The sample consisted of 32 healthy subjects (7 men, 25 women), right-handed and with no personal or familial history of neurological or psychiatric conditions. The lexical decision paradigm employed in this study is constituted by 408 stimuli, 240 words and 168 pseudo-words. The criteria for word selection were: frequency, length, grammatical category and morphology. Electroencephalogram (EEG) monopolar recording was obtained from 19 derivations as well as event-related potentials (ERPs) for the word and pseudo-word sub-states. When analyzing behavioral aspects, subjects made more mistakes when presented with words. However, individuals with less education were the ones committing more mistakes. From this we can infer that this variable may be associated with the range of lexical repertoire. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Balderas C., Ma. Esther and  Galindo y Villa M., Gabriela and  Villa, M. and  Garcell, Josefina Ricardo and  Heinze M., Gerhard},
