@article{1997-06771-00719970601,
Abstract = {Older adults (aged 60–80 yrs) performed 3 lexical information-processing tasks approximately 1.3 times slower than young adults (aged 18–22 yrs). Slopes of regressions based on individual Ss' reaction times (RTs) on single lexical decision (SLD) and category judgment (CJ) tasks did not differ from slopes based on a double lexical decision (DLD) task. Moreover, slopes based on the SLD and CJ tasks accurately predicted the size of semantic priming effects on the DLD. This was true for the older group as a whole, and also for subgroups of fast, medium and slow older adults, as well as for young adult subgroups. The size of the semantic priming effects for the fast old and slow young subgroups were approximately equal, consistent with the idea that the effect of age on priming is entirely attributable to slowing. Across all tasks, each old subgroup showed the same degree of slowing relative to the corresponding young subgroup, so that the differences in RTs observed between subgroups in the young sample were magnified in the old sample. The findings suggest that ability-related differences in lexical processing speed may be functionally equivalent to age-related differences and that both factors interact to determine performance on speeded lexical tasks. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Myerson, Joel and  Hale, Sandra and  Chen, Jing and  Lawrence, Bonnie},
ISSN = {0001-6918, 1873-6297},
Journal = {Acta Psychologica},
Keywords = {age & ability, speeded lexical decision, 18–22 vs 60–80 yr olds, Adult, Aged, Aging, Analysis of Variance, Humans, Linear Models, Mental Processes, Psycholinguistics, Psychological Theory, Reaction Time, Reading, Age Differences, Cognitive Ability, Lexical Decision, Reaction Time},
Number = {1-2},
Pages = {83 - 101},
