Keywords = {aphasia, short term memory span performance, words, numbers, verbal span, nonverbal span, lexical ability, semantic processes, phonological processes, Adult, Aged, Analysis of Variance, Aphasia, Humans, Memory, Short-Term, Middle Aged, Neuropsychological Tests, Phonetics, Psycholinguistics, Semantics, Speech Perception, Aphasia, Cognitive Processes, Short Term Memory, Stimulus Parameters, Verbal Fluency, Digit Span Testing, Lexical Access, Numbers (Numerals), Phonology, Semantics, Words (Phonetic Units)},
Number = {3},
