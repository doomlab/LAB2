Title = {Lebensalterspezifische Veränderungen phonologischer Defizite bei Lese-Rechtschreibstörung = Age dependent changes in phonological reading in dyslexia},
URL = {petra.georgiewa@charite.de},
Volume = {33},
Year = {2004},
