Title = {On priming by a sentence context},
Volume = {112},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=1983-27202-001&site=ehost-live&scope=site},
Year = {1983},
}


@article{1983-22559-00119821101,
Abstract = {Theories of visual word recognition have proposed that a word's phonological properties can be involved in reading visually presented words. It is commonly supposed that this phonological information can be arrived at in at least 2 ways: by looking it up after identifying the word visually (a lexical route) or by rule-governed translating of the word's orthographic code (a nonlexical route). Four experiments were conducted with 96 college students to examine whether phonological information is automatically accessed in visual word recognition, and if so, how this occurs. A priming technique was used with a display sequence of mask, prime, target, mask. Ss were asked to make written responses to any words that they thought were present, and prime identification was minimal. A facilitatory effect of phonological priming on target identification occurred when primes were homophones of targets. However, no similar identification occurred when the prime was a nonword homophone of the target. The homophone priming effect was found irrespective of whether targets followed the spelling-to-sound rules of English. Results suggest that automatic access to phonology can occur in visual recognition and that it operates by means of a lexical route. (47 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Humphreys, Glyn W. and  Evett, Lindsay J. and  Taylor, David E.},
ISSN = {0090-502X, 1532-5946},
Journal = {Memory & Cognition},
Keywords = {lexical vs nonlexical automatic access to phonology, visual word recognition, college students, Female, Form Perception, Humans, Male, Pattern Recognition, Visual, Phonetics, Reading, Cognitive Processes, Models, Phonology, Recognition (Learning), Words (Phonetic Units), Visual Perception},
Number = {6},
Pages = {576 - 590},
Title = {Automatic phonological priming in visual word recognition},
Volume = {10},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=1983-22559-001&site=ehost-live&scope=site},
Year = {1982},
}


@article{1983-05383-00119820701,
Abstract = {Administered the Comprehension Test of the Gates-MacGinitie Reading Tests and a deductive reasoning test to 720 students in Grades 5–8. Each item on the deductive reasoning test consisted of a prose passage containing the premises of a basic logical rule and a question that required Ss to evaluate a conclusion drawn from these premises. Ss' scores on the deductive reasoning test were analyzed according to grade level, level of reading comprehension, and type of logical rule. Developmental gains in deductive reasoning abilities were limited to Ss with high reading comprehension, and interesting variations were found in the order of dominance of the logical rules at each grade level and for each reading comprehension group. These variations may reflect developmental changes in grammatical and lexical skills. (28 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
