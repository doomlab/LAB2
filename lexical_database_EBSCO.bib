@article{1997-03632-00119970201,
Abstract = {The dual mechanism hypothesis argues that the apparent contrast in English inflectional morphology, between a rule-based default procedure generating regular past-tense forms and listed irregular forms stored in an associative net, reflects universal constraints on the properties of possible morphological systems. This study investigated whether these constraints can be shown to hold, in the ways predicted by the dual mechanism hypothesis, for Italian past-tense inflection. Exp 1 (with 24 native Italian-speaking university students), using a cross-modal repetition priming task, showed that priming generated by regular inflected pairs does not differ from priming by irregular inflected pairs. Exp 2 (with 20 native speakers of Italian), using an elicitation task where Ss produce past-tense forms for novel verbs, not only showed little evidence for the presence of an underlying default but also demonstrated that phonological similarity can affect both regularized and irregularized elicitation patterns. These results contrast with previous research in English that used the same techniques, and suggest that claims about universal morphological patterns need to be extended to include procedures that combine both high productivity and lexical specificity. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Orsolini, Margherita and  Marslen-Wilson, William},
ISSN = {0169-0965, 1464-0732},
Journal = {Language and Cognitive Processes},
Keywords = {past-tense inflectional morphological patterns in regular & irregular verb repetition priming & elicitation, native Italian speaking college students, test of dual mechanism hypothesis, Inflection, Morphology (Language), Phonology, Priming, Verbs},
