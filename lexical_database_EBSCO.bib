Keywords = {use of perceptual fluency & memory in creation of 2-way illusions with words & voices in presence of noise, college students, Auditory Perception, Humans, Memory, Noise, Vocabulary, Voice, Illusions (Perception), Memory, Perception, Voice, Words (Phonetic Units), Noise Effects},
Number = {2},
