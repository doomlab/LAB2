Number = {4},
Pages = {227 - 235},
Title = {The effect of word length and other sublexical, lexical, and semantic variables on developmental reading deficits},
URL = {m.deluca@hsantalucia.it, ORCID: 0000-0003-0084-3772, ORCID: 0000-0002-8553-7717, ORCID: 0000-0002-6351-2455},
Volume = {21},
