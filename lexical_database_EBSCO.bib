@article{1986-14092-00119841001,
Abstract = {In Exp I, a sentence classification task was used to determine whether adults use comprehension strategies such as the actor-action-object strategy proposed by T. G. Bever (1970). 24 undergraduates were presented with a number of sentences and were asked to decide as quickly as possible whether or not the sentence made sense. Findings indicate that Ss tended to use Bever's actor-action-object strategy. Exp II examined the possibility that even the weak relations between the words of the sentences used in Exp I could lead to a substantial effect of lexical priming. Findings are consistent with those from Exp I, and overall results provide strong evidence for the presence of lexical priming effects during comprehension. By eliminating the possibility of Ss responding on the basis of decisions made at the sentential level, Exp II demonstrated that the facilitatory effect must have arisen from contextual factors operating at the lexical level. (17 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Norris, Dennis},
