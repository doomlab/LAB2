@inbook{2000-07493-00520000101,
Abstract = {The investigation of the neural basis of language occupies a unique place in the history of both neurology and neuroscience. It is fair to say that neurology began in earnest about a century and a half ago, around the study of language, and that what we now call neuroscience can be traced to that same effort. In this chapter, the authors take this dual status of language into account. They discuss language from the perspective of both neurology and neuroscience, mindful of the significance of language for both clinical practice and neuroscientific progress. Specifically, the authors present a number of findings relating a variety of language processes to a host of neural structures and operations. In an attempt to provide a perspective to interpret those findings, they briefly present a framework for discussing the neural basis of concepts and words. Previous evidence shows that the retrieval of knowledge for concrete entities from different conceptual categories depends on partially segregated neural systems. These findings have clarified further the neural correlates of knowledge retrieval; in this chapter, the authors outline their notion of how the neural circuitry subserving retrieval of conceptual and lexical knowledge operates. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Damasio, Antonio R. and  Damasio, Hanna and  Mesulam, M.-Marsel},
Booktitle = {Principles of behavioral and cognitive neurology., 2nd ed.},
ISBN = {0-19-513475-3},
Pages = {294 - 315},
Publisher = {Oxford University Press},
Title = {Aphasia and the neural basis of language},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2000-07493-005&site=ehost-live&scope=site},
Year = {2000},
