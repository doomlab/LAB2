Year = {1979},
}


@article{1979-25711-00119781001,
Abstract = {Following C. A. Ferguson and C. G. Farwell (1975), the present paper examines phonological development in lexical terms and extends their method to consideration of syllables within words. Ferguson and Farwell dealt with word-initial consonants only; the attempt is made here to determine whether the sets of phone classes in all positions are the same, whether differences that occur from position to position are primarily within the phone class or in the number and distribution of phone classes at each level in the tree, and what effect position in the syllable has on phone classes. Research was directed toward a solution to the problem of how children acquire a sound system: whether they follow 'universal' orders of acquisition (R. Jakobson, 1942) or whether they develop distinct 'strategies' that they may utilize to varying degrees. The 4 children in this study did not all follow the same order of phonological acquisition and among the 4 Ss, at least 3 different strategies were observable. These strategies are examined in detail and discussed with respect to the lexical parameter. (7 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Shibamoto, J. S. and  Olmsted, D. L.},
ISSN = {0305-0009, 1469-7602},
