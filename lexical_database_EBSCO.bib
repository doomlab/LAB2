Title = {Stuttering and lexical retrieval},
URL = {a.packman@cchs.usyd.edu.au},
Volume = {15},
Year = {2001},
}


@article{2002-10274-00120010901,
Abstract = {Examined the role of contextual distinctiveness (CD), a corpus-derived summary measure of the frequency distribution of the contexts in which a word occurs, as a predictor of lexical processing effort. Lexical decision responses were recorded as 18 individuals were presented with 165 word and 165 nonword stimuli. Results show that CD was a significantly better predictor of lexical decision latencies than occurrence frequency, suggesting that CD is the more psychologically relevant variable. Investigation of the relationship between CD and the 6 subjectively defined measures of concreteness, context availability, number of contexts, ambiguity, age of acquisition, and familiarity found CD to be reliably related to ambiguity only. It is concluded that CD is a more cognitively insightful variable than other lexical variables, particularly word frequency. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
