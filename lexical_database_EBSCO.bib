}


@article{1978-06575-00119770601,
Abstract = {To study the notion of stage processing independence in reading, children were given a pronunciation task to measure the decoding process and a controlled association task to measure semantic processing. Errors on each task were analyzed in terms of word properties, pretask familiarization, and grade. Word frequency and, to a lesser extent, word concreteness affected both tasks, while letter variables only made a difference in decoding. Vowel complexity hampered decoding only when words were uncommon. Prefamiliarizations did not have global effects but were specific to the kind of task. Results suggest that, at the word level, decoding is dependent on letter properties as well as familiarity of whole words. The semantic process appeared to be affected by lexical accessibility. (28 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Mason, Jana M.},
ISSN = {0022-0663, 1939-2176},
Journal = {Journal of Educational Psychology},
Keywords = {independence of semantic & decoding processing stages in reading, elementary school students, Articulation (Speech), Cognitive Processes, Elementary School Students, Reading, Word Meaning},
Number = {3},
Pages = {288 - 297},
Title = {Questioning the notion of independent processing stages in reading},
Volume = {69},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=1978-06575-001&site=ehost-live&scope=site},
Year = {1977},
}


@article{1978-04930-00119770701,
Abstract = {Attempted to determine whether mechanically generated sentences, often referred to in the literature as 'anomalous,' are in fact anomalous. A total of 40 undergraduates in 2 experiments were asked to do 2 different tasks: a paired-associates learning task involving anomalous sentences as responses, and a 2nd task involving interpretation of such sentences. Results of the interpretation task show that a large proportion of Ss were able to give interpretations for anomalous sentences; learning results indicate that anomalous sentences were more difficult to anticipate than natural sentences only when task order was Learn first, Interpret second. In the order Interpret-Learn, differences in ease of learning between anomalous and naturally occurring sentences did not appear. Results indicate that anomalous sentences are interpretable, that a range of difficulty for such sentences can be established, that anomalous sentences are learned as easily as naturally occurring ones after interpretation, and that many of the interpretations given to such sentences are metaphoric in character. Findings are discussed in terms of their implications for models of lexical organization. (23 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Pollio, Howard R. and  Burns, Barbara C.},
ISSN = {0090-6905, 1573-6555},
Journal = {Journal of Psycholinguistic Research},
Keywords = {interpretability & learning of anomalous sentences, college students, Meaning, Sentences, Verbal Learning},
Number = {3},
