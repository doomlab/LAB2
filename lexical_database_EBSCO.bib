Keywords = {QUERYING (Computer science), DATABASE management software, RELATIONAL databases, SQL, QUERY languages (Computer science), COMPUTER algorithms, Inverted index, NoSQL indexing, Semantic network, Semantic queries, Semantic-aware data processing, Textual databases},
Pages = {133 - 173},
Title = {Full-fledged semantic indexing and querying model designed for seamless integration in legacy RDBMS.},
