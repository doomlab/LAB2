Year = {1983},
}


@article{1985-06198-00119840301,
Abstract = {Investigated the contributions to semantic priming of 2 components of the semantic representation underlying a word: perceptually and conceptually based information. Perceptual information is based on physical attributes such as shape or color, while conceptual information consists of more abstract elements such as functional attributes. Whether both components would produce an effect in semantic priming was investigated in 2 studies with 56 undergraduates. Pairs of words either related because of a conceptual property (banana-apple), a perceptual property (ball-apple), or both because of a perceptual and conceptual property (cherry-apple) were presented as prime and target in a lexical decision and a word-naming task. Results show independent contributions of perceptual and conceptual attributes to semantic priming. Word recognition was not only influenced by the conceptual similarity to a preceding word but also by the correspondence with the preceding word with regard to perceptually based properties. The perceptual relatedness of the prime and target seemed to have its own independent effect on lexical processes. (30 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Schreuder, Robert and  Flores D'Arcais, Giovanni B. and  Glazenborg, Ge},
ISSN = {0340-0727, 1430-2772},
