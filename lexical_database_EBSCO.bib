Title = {Eye movements and morphological segmentation of compound words: There is a mouse in mousetrap},
Volume = {16},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2004-10798-013&site=ehost-live&scope=site},
Year = {2004},
}


@article{2004-10798-00920040101,
Abstract = {Three reading comprehension experiments with 88 college students examined the effects of a typographical error in parafoveal vision on aspects of foveal inspection time and saccade targeting. A contingent presentation procedure ensured that typographical errors were restored to their correct form before they were viewed in foveal vision: They were never available for foveal processing. In Exp 1, the error was formed by replacing the first letter of the target word with a second occurrence of its second letter, producing an illegal nonword. This manipulation had no significant effect on foveal inspection time, but lowered the probability that a short word prior to the target would be skipped. In Exp 2 the familiarity of the target's initial letters was maintained constant across conditions. This manipulation removed the target 1 skipping effect, suggesting that the outcome of Exp 1 was due to orthographic rather than lexical illegality, but revealed shorter foveal inspection times as a function of the presence of the error. Exp 3 manipulated lexical and sublexical properties of the parafoveal typing error. Properties of the parafoveal error again influenced prior foveal inspection times. Results suggested that the determining properties were sublexical rather than lexical. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Pynte, Joël and  Kennedy, Alan and  Ducrot, Stèphanie},
ISSN = {0954-1446, 1464-0635},
Journal = {European Journal of Cognitive Psychology},
Keywords = {reading comprehension, typographical errors, parafoveal vision, inspection time, saccade targeting, eye movements, Errors, Eye Fixation, Eye Movements, Foveal Vision, Reading Comprehension},
Number = {1-2},
Pages = {178 - 202},
Title = {The influence of parafoveal typographical errors on eye movements in reading},
URL = {pynte@up.univ-aix.fr},
Volume = {16},
Year = {2004},
}


@article{2004-10393-00220040101,
Abstract = {Previous research has shown large response time costs (in excess of 50 ms) when bilingual speakers switch predictably back and forth between naming items (a productive switching task) in their first (L1) and second languages (L2). A recent study using event-related potentials (ERPs) has shown that switching between languages is associated with activity over frontal (N2) and parietal (late positive complex) areas of cortex (Jackson, Swainson, Cunnington, & Jackson, 2001). Switching between naming in different languages requires a switch in both language representations and language-specific motor responses. The current study investigated a receptive (input) language-switching task with a common manual response. Number words were presented in L1 and L2, and participants were required to judge whether the words were odd or even (a parity judgement). Response costs were considerably reduced, and the frontal and parietal switch related activity reported in the productive switching task was absent. Receptive switching was associated with early switch-related activity over central sensors that were not language specific. These results are discussed in relation to the idea that there is no language-specific lexical selection mechanism... (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
Author = {Jackson, G. M. and  Swainson, R. and  Mullin, A. and  Cunnington, R. and  Jackson, S. R.},
ISSN = {0272-4987, 1464-0740},
