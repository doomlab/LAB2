Journal = {Neuropsychologia},
Keywords = {morphosyntactic processing deficit, developmental dyslexia, event related potentials, acoustic impairments, phonological impairments, Acoustic Stimulation, Adult, Analysis of Variance, Articulation Disorders, Brain Mapping, Contingent Negative Variation, Dyslexia, Electroencephalography, Evoked Potentials, Auditory, Female, Humans, Male, Reading, Speech Perception, Statistics as Topic, Young Adult, Acoustics, Dyslexia, Phonology, Evoked Potentials, Syntax},
Number = {8},
Pages = {1595 - 1607},
Title = {Characterizing the morphosyntactic processing deficit and its relationship to phonology in developmental dyslexia},
URL = {chiara.cantiani@bp.lnf.it, ORCID: 0000-0001-6482-6627, ORCID: 0000-0003-4640-306X, ORCID: 0000-0002-4470-4187, ORCID: 0000-0003-0678-4697},
