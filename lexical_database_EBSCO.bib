Year = {1998},
}


@article{1998-10876-00619980801,
Abstract = {Reports 6 studies on late closure in the resolution of attachment ambiguities in Spanish against a backdrop of principle-motivated vs frequency-based models of sentence parsing. All Ss were Spanish speaking adults. Results of a questionnaire study on attachment preferences and 3 self-paced reading experiments show a clear preference towards low attachment (late closure), thus supporting principle-grounded theories. In addition, two corpus studies involving written Spanish sentences were carried out to obtain records of relative frequencies of the attachment choices involved in initial experiments. A coarse-grained measure revealed that, in accordance with linguistic tuning, low-attachment structures are more common in Spanish NP-VP1-NP-VP2-PP sentences. However, a fine-grained count showed that low-attachment preferences cannot be explained by arguing that the specific verbs positioned lower on the tree (VP2) are more likely to take an extra argument than those located at a higher position (VP1), as lexical models would assume. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
Author = {Igoa, José M. and  Carreiras, Manuel and  Meseguer, Enrique},
