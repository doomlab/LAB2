ISSN = {0278-7393, 1939-1285},
Journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
Keywords = {word frequency & rated age of acquisition, oral reading latencies & word pronunciation durations, college students, Wales, Age Differences, Language Development, Oral Reading, Pronunciation, Word Frequency, Word Recognition},
