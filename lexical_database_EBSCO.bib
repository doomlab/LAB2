Keywords = {word electrophysiological responses, event-related potentials, word expectancy vs meaningfulness continuous variation, word ERP averages, Adult, Brain, Brain Mapping, Comprehension, Electroencephalography, Evoked Potentials, Female, Humans, Male, Reading, Recognition (Psychology), Semantics, Evoked Potentials, Stimulus Parameters, Word Recognition, Electrophysiology, Meaning, Words (Phonetic Units)},
Number = {2},
Pages = {137 - 153},
Title = {Parametric analysis of event-related potentials in semantic comprehension: Evidence for parallel brain mechanisms},
URL = {jdien@tulane.edu},
Volume = {15},
Year = {2003},
}


@article{2003-01357-01620030101,
Abstract = {When we read a text, typical sequences of fixations of words form a rather complicated trajectory-almost like a random walk. This complex behavior is the result of the interplay between lexical processing and programming of saccadic eye movements, the two processes that drive eye movements during reading. Stochastic fluctuations are present in both subsystems. We use a minimal three-state model with residence time-dependent transition probabilities to study the role of stochasticity. In particular, we investigate the influence of noise on reading time by numerical simulations. Our model suggests that performance in reading is enhanced by an intermediate level of noise in lexical processing. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
