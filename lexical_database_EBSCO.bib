Journal = {Journal of Fluency Disorders},
Keywords = {lexical priming, young children, stuttering, perceptual similarity, conceptual similarity, words, picture naming task, Analysis of Variance, Case-Control Studies, Child, Preschool, Concept Formation, Female, Humans, Male, Phonetics, Photic Stimulation, Reaction Time, Semantics, Social Class, Speech Perception, Stuttering, Task Performance and Analysis, Lexical Access, Priming, Stimulus Similarity, Stuttering, Naming, Words (Phonetic Units)},
Number = {4},
Pages = {303 - 324},
Title = {Effects of perceptual and conceptual similarity in lexical priming of young children who stutter: Preliminary findings},
