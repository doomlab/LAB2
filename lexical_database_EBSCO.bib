Pages = {239 - 283},
Publisher = {Elsevier Academic Press},
Series = {The psychology of learning and motivation; Vol 67; ISSN: 0079-7421 (Print)},
Title = {Context as an organizing principle of the lexicon},
URL = {jonesmn@indiana.edu},
Volume = {67},
Year = {2017},
}


@inbook{2017-14733-00620170101,
Abstract = {In this paper, we explore the generation of past-tense verbs in a case of a man with a semantic disorder and a marked anomia. He also produced frequent slips of the tongue in short-term memory tasks, involving sets of single words and short sentences. In our investigation, we used the standard Today-Yesterday task but, rather than using mixed lists of regular and irregular verbs, as are generally used, we blocked the regular and irregular verbs according to their phonological properties. We began by presenting an ordered series of blocked lists, but later developed a new technique in which we established one type of verb before mixing it with others. The results of our investigations revealed interactions between different types of verbs that we believe expose the phonological-morphological frames of each type of verb, and place them all within a dynamic lexical system. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
