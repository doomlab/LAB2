@article{2011-06405-00220110101,
Abstract = {Background: The effects of stimulus material, lexical content, and response condition on dichotic speech recognition performance characteristics were examined for normal-hearing young adult listeners. No previous investigation has systematically examined the effects of stimulus material with constant phonetic content but varied lexical content across three response conditions typically used to evaluate binaural auditory processing abilities. Purpose: To examine how dichotic speech recognition performance varies for stimulus materials with constant phonetic content but varied lexical content across the free recall, directed recall right, and directed recall left response conditions. Research Design: Dichotic speech recognition was evaluated using consonant-vowel-consonant (CVC) word and nonsense CVC syllable stimuli administered in the free recall, directed right, and directed left response conditions, a repealed measures experimental design. Study Sample: Thirty normal-hearing young adults (15 male, 15 female) served as participants. Participants ranged in age from 18 to 31 yr and were all right-handed. Data Collection and Analysis: Participants engaged in monaural speech recognition and dichotic speech recognition, tasks. Percent correct recognition per ear, as well as ear advantage for dichotic speech recognition, were calculated and evaluated using a repeated measures analysis of variance (ANOVA) statistical procedure. Results: Dichotic speech recognition performance for nonsense CVC syllables was significantly poorer than performance for CVC words, suggesting that lexical content impacts performance on dichotic speech recognition tasks. Performance also varied across response condition, which is consistent with previous studies of dichotic speech recognition. Conclusions: Lexical content of stimulus materials impacts performance characteristics for dichotic speech recognition tasks in the normal-hearing young adult population. The use of nonsense CVC syllable material may provide a way to assess dichotic speech recognition performance while potentially lessening the effects of lexical content on performance. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Findlen, Ursula M. and  Roup, Christina M.},
ISSN = {1050-0545, 2157-3107},
Journal = {Journal of the American Academy of Audiology},
Keywords = {dichotic speech recognition, consonant-vowel-consonant, words, nonsense syllables, Acoustic Stimulation, Adolescent, Adult, Auditory Perception, Dichotic Listening Tests, Female, Humans, Male, Phonetics, Speech Perception, Young Adult, Consonants, Dichotic Stimulation, Speech Perception, Syllables, Vowels, Words (Phonetic Units)},
Number = {1},
Pages = {13 - 22},
Title = {Dichotic speech recognition using CVC word and nonsense CVC syllable stimuli},
URL = {ursula.findlen@gmail.com},
Volume = {22},
Year = {2011},
}


@article{2011-04981-00120110101,
Abstract = {In multiple properly models of lexical categories, the assignment of a word to a lexical category is determined by a set of properties associated with that category. Specifically, if a word displays a sufficient number of properties associated with a given category, it will be assigned to that category. The current article critically evaluates multiple property models of lexical categories. In particular, it focuses on a multiple property model recently developed by A arts (2007b) and a variety of other such models, It is argued that multiple property models encounter problems in four key areas. Firstly, they struggle to determine an appropriate set of lexical categories and lo provide coherent accounts of the nature and function of such categories. Secondly, they encounter difficulties in determining the properties that define categories and in determining features of properties such as whether they should be weighted or not. Thirdly, a variety of problems arise when multiple property models combine words, properties and categories together. Finally, multiple property models fail to account for crosslinguistic data. It is concluded that multiple property models do not provide viable accounts of lexical categories. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Smith, Mark},
ISSN = {0024-3949, 1613-396X},
Journal = {Linguistics},
Keywords = {multiple property models, lexical categories, words, linguistics, Linguistics, Lexical Access, Models, Words (Phonetic Units)},
Number = {1},
Pages = {1 - 51},
Title = {Multiple property models of lexical categories},
URL = {drmcsmith@hotmail.com},
Volume = {49},
Year = {2011},
}


@article{2011-00185-00820110101,
Abstract = {Comments on an article by Carol Stoel-Gammon (see record [rid]2011-00185-002[/rid]). Stoel-Gammon provides a welcome addition to the phonological acquisition literature, bringing together insights from long-standing and more recent research to address the relationship between the developing phonological system and the developing lexicon. As Stoel-Gammon notes, both phonotactic probability and neighborhood density play a role in understanding the nature of the developing lexicon. This is part of a more general phenomenon that relates the characteristics of the developing lexicon to several levels of phonological structure. This is easier to see from a cross-linguistic perspective, where learners' patterns of both syllable structure and lexical development appear to be closely tied to the frequency of the syllable structure and prosodic word shapes in the ambient language. Children's variable production of both grammatical morphemes and coda consonants is also influenced by the position in which the lexical item occurs in the utterance. Ultimately, these processes will be better understood in the context of a developmental theory of language planning and production, where phonological knowledge and implementation are considered as part of lexical and grammatical competence. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Demuth, Katherine},
ISSN = {0305-0009, 1469-7602},
Journal = {Journal of Child Language},
Keywords = {lexical development, phonological development, young children, psychologists, individual differences, word learning, Child Language, Child, Preschool, Humans, Language Development, Linguistics, Multilingualism, Phonetics, Semantics, Speech, Vocabulary, Individual Differences, Learning, Lexical Access, Phonological Awareness, Childhood Development, Psychologists, Words (Phonetic Units)},
Number = {1},
Pages = {69 - 74},
Title = {Interactions between lexical and phonological development: Cross-linguistic and contextual considerations—A commentary on Stoel-Gammon's 'Relationships between lexical and phonological development in young children'},
URL = {katherine.demuth@mq.edu.au},
Volume = {38},
Year = {2011},
}


@article{2011-00185-00520110101,
Abstract = {Comments on an article by Carol Stoel-Gammon (see record [rid]2011-00185-002[/rid]). The author states that 'from birth to age 2; 6, the developing phonological system affects lexical acquisition to a greater degree than lexical factors affect phonological development'. This conclusion is based on a wealth of data; however, the available data are somewhat limited in scope, focusing on rather holistic measures of the phonological and lexical systems. He suggests a number of important avenues to pursue, but does not discuss a critical one that is emerging in the broader literature on word learning It is likely that triggering is more accurate and efficient when the mismatch between the input and existing representations is maximal, far exceeding the threshold for allocating a new representation. It is likely that configuration is dependent on working memory. That is, a novel sound sequence must be held in working memory so that the details of the sound sequence can be stored in long-term memory. In terms of engagement, the most compelling evidence comes from studies showing changes in responses to trained non words across a one week interval without further training. A more precise statement, based on additional research, has the potential to enhance our understanding of the relationship between phonological and lexical development across the lifespan. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Storkel, Holly L.},
ISSN = {0305-0009, 1469-7602},
Journal = {Journal of Child Language},
Keywords = {lexical development, phonological development, young children, psychologists, individual differences, word learning, Child, Preschool, Humans, Language Development, Learning, Memory, Phonetics, Semantics, Individual Differences, Learning, Lexical Access, Phonological Awareness, Childhood Development, Psychologists, Words (Phonetic Units)},
Number = {1},
Pages = {51 - 55},
Title = {Differentiating word learning processes may yield new insights—A commentary on Stoel-Gammon's 'Relationships between lexical and phonological development in young children'},
URL = {hstorkel@ku.edu, ORCID: 0000-0001-6533-6224},
Volume = {38},
Year = {2011},
}


@article{2011-00185-00420110101,
Abstract = {Comments on an article by Carol Stoel-Gammon (see record [rid]2011-00185-002[/rid]). According to the prevailing view at the time, accuracy of pronunciation was a peripheral phenomenon that had nothing to do with the development of language qua language. One reason why not, according to Stoel-Gammon (SG), is that the difficulty of accurately producing sounds influences the words children acquire and the rate at which they acquire them. This interaction between the articulatory skill of children and phonological properties of words is just one of the mutual influences between phonology and the lexicon SG describes. In her target article, SG brings together data from a wide range of investigations to build an account of how phonology and the lexicon interact in development. The evidence SG marshals in support of this account includes very recent findings and findings that have been around for a while but have never been incorporated into a larger explanatory structure. SG also brings input into the story, arguing that input affects articulation and that these effects demonstrate the child's sensitivity to the structure of the ambient language. Another topic SG raises for us to consider is the well-established relation between non word repetition accuracy and word learning. SG also endorses the view that lexical development drives phonological development. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Hoff, Erika and  Parra, Marisol},
ISSN = {0305-0009, 1469-7602},
Journal = {Journal of Child Language},
Keywords = {lexical development, phonological development, young children, psychologists, individual differences, word learning, Child Language, Child, Preschool, Humans, Language Development, Phonetics, Semantics, Speech, Speech Intelligibility, Vocabulary, Individual Differences, Learning, Lexical Access, Phonological Awareness, Psychologists, Words (Phonetic Units)},
Number = {1},
Pages = {46 - 50},
Title = {Mechanisms linking phonological development to lexical development—A commentary on Stoel-Gammon's 'Relationships between lexical and phonological development in young children'},
URL = {ehoff@fau.edu},
Volume = {38},
Year = {2011},
}


@inbook{2011-23569-04620110101,
Abstract = {Parafoveal-on-foveal effects refer to the possibility that processing of the parafoveal word can influence the fixation durations on the foveal word during reading. In this chapter, I will review the literature of studies examining this issue. Effects observed in reading-like tasks have been questioned on methodological grounds with regards to the generalizability to normal reading. The clearest evidence for the existence of parafoveal-on-foveal effects comes from experiments allowing tight control over the stimuli but restricting the observations to fixation locations very close to the parafoveal word, and from corpus studies showing reliable but numerically small effects. I will make the claim that with regards to taking reported parafoveal-on-foveal effects as evidence for parallel lexical processing, the jury is still out. The argument is that as long as parafoveal-on-foveal effects are numerically small and difficult to replicate in a controlled experiment, they can be explained on the basis of mislocated fixations, machine error, and binocular disparity. These latter three influences can create apparent parafoveal-on-foveal effects without being linked to parallel lexical processing. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Drieghe, Denis and  Liversedge, Simon P. and  Gilchrist, Iain D. and  Everling, Stefan},
Booktitle = {The Oxford handbook of eye movements.},
ISBN = {978-0-19-953978-9},
Pages = {839 - 855},
Publisher = {Oxford University Press},
Series = {Oxford library of psychology},
Title = {Parafoveal-on-foveal effects on eye movements during reading},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2011-23569-046&site=ehost-live&scope=site},
Year = {2011},
}


@article{2010-26725-00420110101,
Abstract = {Lexical frequency and item familiarity definitely influence name retrieval in clinical studies, but the relevance of concept typicality has been less extensively investigated. A relevant theoretical question is whether familiarity and typicality exert an independent influence on the name retrieval of aphasic patients. In this study we assembled a list of 288 items of the animals category from a semantic fluency task performed by 198 normal subjects in a previous study. The production frequency of a given animal name was used to estimate typicality, and for the whole list of animals a new normal sample estimated familiarity. The newly collected indices were used to analyse semantic fluency and picture naming of FPA, an anoxic patient. On both tasks, familiarity and typicality were more relevant than word frequency. Typicality of each stimulus, and to a lower degree familiarity, were independently able to predict the type of naming response. Thus, the influence of lexical-semantic variables on naming should be viewed, at least in this case, as something endowed with two dimensions (typicality and familiarity), while in other cases also lexical frequency may be relevant. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Laiacona, Marcella and  Scotti, Stefania and  Allamano, Nadia and  Lorenzi, Lorena and  Capitani, Erminio},
ISSN = {0911-6044},
Journal = {Journal of Neurolinguistics},
Keywords = {semantics, name production, lexical frequency, familiarity, anoxia, word frequency, Anoxia, Familiarity, Naming, Semantics, Word Frequency, Lexical Access},
Number = {1},
Pages = {70 - 88},
Title = {Exploring the semantic category of animals: Some useful indices for a qualitative study of name production and their validation in a patient},
URL = {marcella.laiacona@fsm.it, erminio.capitani@unimi.it},
Volume = {24},
Year = {2011},
}


@article{2010-25622-00420110101,
Abstract = {This paper investigated the predictive ability of expressive vocabulary size and lexical composition at age 2 on later language and literacy skills from ages 3 through 11. Multivariate analysis of covariance was performed to compare 16 language and literacy outcomes between children with large expressive vocabulary size at 24 months (N = 1,073) and those with smaller expressive vocabulary size. Comparisons between large and small verb size groups as a measure of lexical composition were also conducted. Our findings indicate that, after controlling for gender, birth order, ethnicity and socioeconomic status, total vocabulary size at age 2 can significantly predict subsequent language and literacy achievement up to fifth grade. Moreover, vocabulary size is a better predictor of later language ability than lexical composition. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
