Number = {2},
Pages = {409 - 423},
Title = {Developmental changes in ERP correlates of spoken word recognition during early school years: A phonological priming study},
URL = {m.bonte@psychology.unimaas.nl},
Volume = {115},
Year = {2004},
}


@article{2005-01440-00320040101,
Abstract = {Background: The clinical symptoms of dyslexia change over the life span of the patients and the age-dependent changes in reading ability are different from those seen during normal reading development. Objective: Measuring age differences in reading performance of dyslexic and normally reading persons in tasks requiring different reading strategies. Methods: 102 patients with dyslexia and 91 normally reading controls (children, adolescents, and adults) had to read aloud material adapted for the respective age group: (1) words; (2) nonwords; and (3) a phonological transformation task. Reading performance was quantified by errors and time taken. Results: Dyslexic patients performed significantly more poorly on all three tasks significantly than did the matched controls. In both groups a significant improvement of word reading appeared with increasing age. Essential improvement of reading performance was observed in the normally reading group for nonword reading and phonological transformation only. Conclusions: The abilities of adults with dyslexia to carry out grapheme-phoneme transformations developed more slowly than in normals. Compensation of reading deficits seems to be possible by means of whole word (lexical) reading strategies. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Georgiewa, Petra and  Grünling, Carolin and  Ligges, Marc and  Filz, Christina and  Möller, Ulrich and  Blanz, Bernhard},
ISSN = {1616-3443, 2190-6297},
