Title = {Syntactic and semantic coordination in finite complement-clause constructions: A diary-based case study},
URL = {bahar_koymen@eva.mpg.de},
Volume = {43},
Year = {2016},
}


@masterthesis{2016-17131-29220160101,
Abstract = {Exposure to L2 orthography may help L2 learners distinguish a novel segmental (e.g., Escudero et al., 2008) and tonal contrast (Showalter & Hayes-Harb, 2013). However, little is known about whether learners can associate two phonetic variants with the same lexical entry when the orthographic form of the word is provided during learning. Thus, the goal of this dissertation is to investigate whether exposure to L2 orthography could help learners link two free variants with one lexical entry and whether Taiwanese and American participants will show different degrees of reliance on orthography in word learning. Four experiments were conducted in the current study. Experiment 1 examined whether orthography can help learners learn the [?-u] free variation in an artificial language using a word learning paradigm modified from Hayes-Harb, Nicol, and Barker (2010). No effect of orthography on the learning of vocalic free variation was found in Experiment 1, and both L1 groups showed low detection of the vowel alternations. A follow-up picture naming task investigating learners' encoding and decoding of the words was conducted in Experiment 2. The results from Experiment 2 showed that orthography did help learners score higher in picture naming and also produce more new forms of the free variants. Experiment 3 and 4 paralleled 1 and 2, respectively, but the target alternation was consonantal (i.e., [p-b] or [t-d] counterbalanced). An effect of orthography on the learning of [p-b] free variation (but not [t-d]) was found in Experiment 3. Compared to Experiment 1, only Americans showed better detection of the consonantal alternations but not Taiwanese. Experiment 4, a picture-naming task, was a follow-up of Experiment 3. It was found that, unlike in Experiment 2, orthography in this case did not always help learners score higher in production. In sum, mixed results regarding the effect of orthography on the learning of free variation were found. In general, the effect of orthography was stronger in production than in recognition. The findings reported in this dissertation add support to the role of orthographic information in lexical access. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Yang, Chung-Lin},
Keywords = {phonological variation, word learning, orthography, word recognition, Phonology, Learning},
School = {ProQuest Information & Learning},
Title = {Phonological variation and l2 word learning: The role of orthography in word recognition and production},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2016-17131-292&site=ehost-live&scope=site},
Year = {2016},
}


@article{2015-39779-00120151201,
Abstract = {In printed-word perception, the orthographic neighborhood effect (i.e., faster recognition of words with more neighbors) has considerable theoretical importance, because it implicates great interactivity in lexical access. Mulatti, Reynolds, and Besner Journal of Experimental Psychology: Human Perception and Performance, 32, 799–810 (2006) questioned the validity of orthographic neighborhood effects, suggesting that they reflect a confound with phonological neighborhood density. They reported that, when phonological density is controlled, orthographic neighborhood effects vanish. Conversely, phonological neighborhood effects were still evident even when controlling for orthographic neighborhood density. The present study was a replication and extension of Mulatti et al. (2006), with words presented in four different formats (computer-generated print and cursive, and handwritten print and cursive). The results from Mulatti et al. (2006) were replicated with computer-generated stimuli, but were reversed with natural stimuli. These results suggest that, when ambiguity is introduced at the level of individual letters, top-down influences from lexical neighbors are increased. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
Author = {Barnhart, Anthony S. and  Goldinger, Stephen D.},
ISSN = {1069-9384, 1531-5320},
Journal = {Psychonomic Bulletin & Review},
Keywords = {Word perception, Orthography, Phonology, Visual word recognition, Adult, Humans, Linguistics, Pattern Recognition, Visual, Psychomotor Performance, Reading, Young Adult, Orthography, Phonology, Visual Perception, Word Recognition},
Number = {6},
Pages = {1739 - 1745},
Title = {Orthographic and phonological neighborhood effects in handwritten word perception},
URL = {abarnhart@carthage.edu},
Volume = {22},
Year = {2015},
}


@article{2016-00367-00320151101,
Abstract = {In Latin, a short vowel followed by a voiced plosive undergoes lengthening in the past participle (Lachmann’s law). Many of these ppp. show a nasal segment inherited from the present or perfect stem. In a few ppp. moving from a dental stem, such as pēnsus, spōnsus etc., such a spreading provides the condition for the formation of the cluster -ns- (< -nd- + -to). By observing the supposed omission of -n- in some of these verbal forms (e.g.: frēsus vs. frĕndo), regardless of the effective status of the process, as opposed to its retention in other ones (e.g.: pēnsus vs. pĕndeo/pĕndo), Latin speakers could have hypothesized that all ppp. forms of this group were originally provided with -n-, see e.g. frēsus < frĕnsus, with loss of -n- and compensatory lengthening of the preceding vowel. The ‘NS-lengthening’ rule might represent an early trigger of the vowel lengthening described by Lachmann. Such a lengthening analogically spread to other ppp., according to a connectionist lexical process. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {De Angelis, Alessandro and  Chilà, Annamaria},
ISSN = {0165-4004},
Journal = {Folia Linguistica: Acta Societatis Linguisticae Europaeae},
Keywords = {Lachmann’s Law, compensatory lengthening, cognitive schemas, lexical connectionism, Pronunciation, Vowels, Cognitive Linguistics, Cognitive Processes},
Number = {Historica 36},
Pages = {89 - 111},
Title = {A new interpretation of Lachmann’s Law},
URL = {adeangelis@unime.it, annamaria.chila@libero.it},
Volume = {49},
Year = {2015},
}


@article{2015-55510-01120151101,
Abstract = {This special issue collects a number of contributions that deal with the role and functions of descriptive norms—that is the perceived common behaviors of others—in social life and how they determine the perceivers’ behaviors. In other words, research on descriptive norms is basically on the norms–behavior link, a link that also has been called conformism. But there is much more to say about the embedding of norms in social life and their transmission. The contributions assembled in this special issue consider communication 'grounding' to clarify the meaning of a word or set of words akin to the communicators subscribing to the same lexical entry of a word. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
