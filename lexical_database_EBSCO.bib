ISSN = {0001-6918, 1873-6297},
Journal = {Acta Psychologica},
Keywords = {spelling recognition, accuracy judgments, word classification, lexical representations, context memory, Adolescent, Adult, Female, Humans, Judgment, Learning, Linguistics, Male, Middle Aged, Recognition (Psychology), Judgment, Lexical Access, Recognition (Learning), Spelling, Word Recognition, Memory},
Number = {3},
