}


@article{2002-12819-01520020301,
Abstract = {Investigated the processing of lexical ambiguity in context in 8 individuals with schizophrenia and a matched control group. Ss made speeded lexical decisions on the 3rd word in auditory word triplets representing concordant (coin-bank-money), discordant (river-bank-money), neutral (day-bank-money), and unrelated (river-day-money) conditions. Results show that when the interstimulus interval between the words was 100 msec, Ss with schizophrenia demonstrated priming consistent with selective, context-based lexical activation. At 1250 msec interstimulus interval, there was a pattern of nonselective meaning facilitation. These results suggest an attentional breakdown in the sustained inhibition of meanings on the basis of lexical context. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
