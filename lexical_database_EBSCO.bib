Journal = {Language, Cognition and Neuroscience},
Keywords = {prosody, speech segmentation, event-related potentials, temporal attention, Auditory Evoked Potentials, Prosody, Speech Perception, Syllables, Word Recognition},
Number = {9},
Pages = {1132 - 1146},
Title = {Auditory evoked potentials reveal early perceptual effects of distal prosody on speech segmentation},
URL = {mbreen@mtholyoke.edu},
Volume = {29},
Year = {2014},
}


@article{2014-30852-02320141001,
Abstract = {The current research investigated how infants apply prior knowledge of environmental regularities to support new learning. The experiments tested whether infants could exploit experience with native language (English) phonotactic patterns to facilitate associating sounds with meanings during word learning. Infants (14-month-olds) heard fluent speech that contained cues for detecting target words; the target words were embedded in sequences that occur across word boundaries. A separate group heard the target words embedded without word boundary cues. Infants then participated in an object label learning task. With the opportunity to use native language patterns to segment the target words, infants subsequently learned the labels. Without this experience, infants failed. Novice word learners can take advantage of early learning about sounds to scaffold lexical development. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
