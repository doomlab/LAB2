Journal = {Research in Developmental Disabilities},
Keywords = {orthographic transparency, typical reading development, atypical reading development, dyslexia, visual attention span, phonemic awareness, Attention, Awareness, Case-Control Studies, Child, Child Development, Dyslexia, Female, Humans, Male, Multilingualism, Phonetics, Reading, Visual Perception, Orthography, Phonological Awareness, Reading Development, Attention Span, Bilingualism, Dyslexia, Visual Attention},
Number = {5},
Pages = {1177 - 1190},
Title = {Impact of orthographic transparency on typical and atypical reading development: Evidence in French-Spanish bilingual children},
URL = {m.lallier@bcbl.eu, ORCID: 0000-0003-4340-1296},
Volume = {35},
