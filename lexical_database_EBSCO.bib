Keywords = {performance on working memory span test & vocabulary & lexical decision & letter matching tasks, prediction of reading comprehension, 18–60 yr olds, Cognitive Ability, Reading Comprehension},
Number = {1},
Pages = {119 - 131},
Title = {Components of fluent reading},
URL = {ORCID: 0000-0003-4532-9312},
Volume = {24},
Year = {1985},
}


@article{1986-23584-00119850701,
Abstract = {Four experiments, with 232 undergraduates, examined whether Ss could utilize subtle acoustical cues in reconstructing words that were missing from the speech signal. Preliminary tests revealed that Ss could make use of semantic context in such reconstruction, analogous to earlier results in reading. In Exp I, Ss were able to use intonational cues at the beginning of a string to detect whether or not a sentence-initial word had been missing. In Exps II and III, Ss utilized coarticulatory cues for palatalization in sentence-internal locations to guide their lexical selection of reconstructed words. In Exp IV, Ss were influenced in lexical selection by the duration of a silent interval that had replaced the missing word. Results indicate that listeners can utilize the acoustical fine structure of speech to both detect the occurrence of a missing word and guide its lexical reconstruction. (29 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
