Keywords = {Rolandic epilepsy, Language impairment, Developmental dysphasia, CELF-4NL, Language profile, Specific language impairment, Dysphasia, Epilepsy, Language Development, Child Characteristics},
Pages = {18 - 28},
Title = {Evaluation of the language profile in children with rolandic epilepsy and developmental dysphasia: Evidence for distinct strengths and weaknesses},
