@article{2006-10224-00820060801,
Abstract = {Short-term memory (STM) for signs in native signers consistently shows a smaller capacity than STM for words in native speakers. Some researchers have argued that articulation time is sufficient to account for the sign-speech difference in STM. If articulation time does not fully account for the sign-speech difference in STM, then other differences between sign and speech may be important. In the article, the results of three experiments are reported. The first two verified that digits yield better STM than letters. In particular, because vision and audition have strikingly different information-processing characteristics, the sign-speech difference could be due to perceptually based coding. The third experiment returned to the original question: whether superiority of spoken language in STM persists when articulatory duration is controlled. It is concluded that STM for American Sign Language (ASL) and STM for English do not in fact differ in underlying capacity. Instead, the usually observed difference in span is due to other factors known to affect STM, most notably articulation time, which differs substantially between ASL and English for most lexical items. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Wilson, Margaret and  Emmorey, Karen},
ISSN = {0956-7976, 1467-9280},
Journal = {Psychological Science},
Keywords = {sign language, short-term memory, speech, words, Cognition, Humans, Memory, Short-Term, Phonetics, Sign Language, Speech, Verbal Behavior, Human Channel Capacity, Oral Communication, Short Term Memory, Sign Language, Words (Phonetic Units)},
Number = {8},
Pages = {682 - 683},
Title = {Comparing Sign Language and Speech Reveals a Universal Limit on Short-Term Memory Capacity},
URL = {mlwilson@ucsc.edu},
Volume = {17},
Year = {2006},
}


