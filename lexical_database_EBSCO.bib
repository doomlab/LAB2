Title = {Locus of semantic interference in picture-word interference tasks},
URL = {m.damian@bristol.ac.uk},
Volume = {10},
Year = {2003},
}


@article{2003-03591-00920030301,
Abstract = {The question of when and how bottom-up input is integrated with top-down knowledge has been debated extensively within cognition and perception, and particularly within language processing. A long running debate about the architecture of the spoken-word recognition system has centered on the locus of lexical effects on phonemic processing: does lexical knowledge influence phoneme perception through feedback, or post-perceptually in a purely feedforward system? J. L. Elman and J. L. McClelland (1988) reported that lexically restored ambiguous phonemes influenced the perception of the following phoneme, supporting models with feedback from lexical to phonemic representations. Subsequently, several authors have argued that these results can be fully accounted for by diphone transitional probabilities in a feedforward system (P. Cairns et al., 1995; M. A. Pitt & J. M. McQueen, 1998). We report results strongly favoring the original lexical feedback explanation: lexical effects were present even when transitional probability biases were opposite to those of lexical biases. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Magnuson, James S. and  McMurray, Bob and  Tanenhaus, Michael K. and  Aslin, Richard N.},
ISSN = {0364-0213, 1551-6709},
Journal = {Cognitive Science},
Keywords = {lexical effects, language understanding, neural networks, language processing, lexical knowledge, phoneme perception, speech perception, coarticulation, Articulation (Speech), Comprehension, Lexical Access, Phonemes, Speech Perception, Language, Lexical Decision, Neural Networks},
Number = {2},
Pages = {285 - 298},
Title = {Lexical effects on compensation for coarticulation: The ghost of Christmash past},
URL = {magnuson@psych.columbia.edu},
Volume = {27},
Year = {2003},
}


@article{2003-02266-00220030301,
Abstract = {In principle, a specific deficit in processing verbs relative to nouns might arise as a result of damage to any of several mechanisms involved in speech planning and lexical production. Here we describe a fluent aphasic patient HG (aged 79 yrs) who is much worse at retrieving verbs relative to nouns in picture naming and sentence generation, but who retains the ability to produce verbal morphology and even to generate novel verbs productively from nominal roots when she is unable to retrieve appropriate action words (e.g. 'looming' for weaving). Moreover, the results of single word and sentence comprehension tasks suggest that her ability to access lexical forms of verbs is sensitive to their thematic properties. When contrasted with patients who have specific deficits in processing morphosyntactic properties of verbs [J. Neurolinguist. 15 (2002) 265], HG's pattern of performance can be seen as evidence that grammatical properties are functionally distinct from other aspects of lexical representation. We discuss the implications of this dissociation, and of HG's performance specifically, in light of various models of lexical access. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Shapiro, Kevin and  Caramazza, Alfonso},
ISSN = {0911-6044},
Journal = {Journal of Neurolinguistics},
Keywords = {phonological properties, verb retrieval deficits, lexical production, aphasic patient, speech planning, lexical access, picture naming, sentence generation, Aphasia, Grammar, Phonology, Verbs, Word Recognition, Lexical Decision, Naming, Nouns},
Number = {2-3},
Pages = {85 - 111},
Title = {Looming a loom: Evidence for independent access to grammatical and phonological properties in verb retrieval},
URL = {caram@wjh.harvard.edu},
Volume = {16},
Year = {2003},
}


@article{2003-05000-01020030201,
Abstract = {A key dispute in theories of spoken word recognition is whether activation of a lexical representation can affect the perception of sublexical components, such as phonemes. J. L. Elman and J. L. McClelland (1988) provided evidence for such top-down processing by showing that a prelexical process (compensation for coarticulation) could be affected by lexical activation. However, M. A. Pitt and J. M. McQueen (1998) reported that the observed compensation effects were in fact due to the transitional probability of certain phonemic sequences, rather than lexical activation. Part I of the current study shows that perceptual grouping must be considered in assessing compensation effects, reopening the question of whether lexical activation can drive compensation. Part II shows that when perceptual grouping is taken into account, lexically mediated compensation can indeed be observed, confirming the interactive nature of speech processing. Collectively, the results make it clear that compensation for coarticulation is affected by a number of factors, making it difficult to isolate lexical influences on the phenomenon. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Samuel, Arthur G. and  Pitt, Mark A.},
ISSN = {0749-596X, 1096-0821},
Journal = {Journal of Memory and Language},
Keywords = {lexical activation, speech perception, spoken word recognition, phonemes, sublexical components, compensation for coarticulation, lexical processing, speech processing, Articulation (Speech), Lexical Access, Speech Perception, Speech Processing (Mechanical), Word Recognition, Lexical Decision, Phonemes},
Number = {2},
Pages = {416 - 434},
Title = {Lexical activation (and other factors) can mediate compensation for coarticulation},
URL = {asamuel@ms.cc.sunysb.edu},
Volume = {48},
Year = {2003},
}


@article{2003-01736-00120030201,
Abstract = {Use of word length for word identification was examined in 3 naming experiments and 1 sentence reading experiment in which a foveally presented cue either matched or mismatched the length of a subsequently presented target word. Properties of the target were also manipulated so that it was either a high- or low-frequency word or so that its exterior letters were either consistent with a large or small pool of candidate words. A total of 114 subjects (Ss) participated across all 4 experiments. The experiments converged in showing that effects of prior word length cuing were either negligible or absent. The word frequency of the target influenced its recognition but not the lexical constraint of its exterior letters. Importantly, effects of target word properties did not interact with effects of prior target length cuing. Together, these results indicate that knowledge of a word's spatial properties does not constrain its lexical processing. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Inhoff, Albrecht W. and  Eiter, Brianna M.},
ISSN = {0340-0727, 1430-2772},
Journal = {Psychological Research},
Keywords = {word length, word identification, foveal cues, word frequency, target word properties, naming, sentence reading, Analysis of Variance, Cues, Humans, Pattern Recognition, Visual, Reaction Time, Reading, Cues, Stimulus Parameters, Word Frequency, Word Recognition, Words (Phonetic Units), Naming, Reading},
Number = {1},
