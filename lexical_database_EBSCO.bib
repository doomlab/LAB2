Journal = {Brain and Language},
Keywords = {left inferior frontal cortex, syntactic processing, aphasia, Aged, Aged, 80 and over, Aphasia, Broca, Comprehension, Discrimination Learning, Female, Frontal Lobe, Functional Laterality, Humans, Language Tests, Male, Middle Aged, Pattern Recognition, Visual, Reaction Time, Semantics, Speech Perception, Time Factors, Verbal Learning, Visual Perception, Vocabulary, Aphasia, Frontal Lobe, Prefrontal Cortex, Syntax},
Number = {3},
