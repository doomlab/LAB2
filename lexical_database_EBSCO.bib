@article{1979-02755-00119780201,
Abstract = {Conducted 6 experiments with a total of 280 undergraduates in which Ss saw and pronounced, either aloud or silently, 7-item lists made from vocabularies of phonologically identical items (write-right-right-rite-write-rite-write). These materials were used to test the predictions of a precategorical and a postcategorical hypothesis for the modality effect (auditory vs visual presentation) in immediate memory. Results support the precategorical hypothesis. Furthermore, performance with these lists shows clearly that short-term memory tasks do not necessarily depend on phonological coding. The last 4 experiments were designed to find out what memory code was used by Ss in this situation; a visual hypothesis was tentatively rejected in favor of an abstract lexical hypothesis. (17 ref) (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Crowder, Robert G.},
