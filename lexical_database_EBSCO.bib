Keywords = {semantic interference, facilitation, Stroop like task, picture-word interference, lexical selection, Adolescent, Adult, Attention, Humans, Intention, Paired-Associate Learning, Pattern Recognition, Visual, Perceptual Masking, Photic Stimulation, Reaction Time, Reference Values, Semantics, Speech, Verbal Behavior, Interference (Learning), Lexical Access, Semantics, Stroop Color Word Test, Competition, Priming},
