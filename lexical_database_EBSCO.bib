ISSN = {0096-1523, 1939-1277},
Journal = {Journal of Experimental Psychology: Human Perception and Performance},
Keywords = {orthographic neighborhood size vs frequency, word recognition & lexical decision, college students, Canada, Lexical Decision, Orthography, Stimulus Frequency, Stimulus Parameters, Word Recognition},
