Year = {2001},
}


@article{2001-14769-01120010101,
Abstract = {Investigated how 6-mo-old infants, who have had a substantial amount of exposure to their native language, perceive lexical and grammatical words. Specifically the authors determined if the categorical discrimination evident in the neonatal period is evident at 6 mo of age, and if so, whether it is being harnessed in any way that might be of more direct relevance for breaking into language. Research was conducted in 3 studies. Ss for studies 1, 2, and 3 were 32, 13, and 13 full-term, healthy, monolingual English-learning 6-mo-olds, respectively. In Study 1, Ss were habituated to a list of either lexical or grammatical words and then tested on new list of words from the same and contrasting categories. The results indicate that the Ss showed recovery to lexical words after habituation to grammatical words (but not vice versa). In Studies 2 and 3, preference was assessed directly by presenting infants with alternating trials of lexical and grammatical words in the central-fixation preference procedure. The results indicate that Ss looked significantly longer during lexical-word than grammatical-word trials. These findings suggest that, by 6 mo, infants attend preferentially to lexical words. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Shi, Rushen and  Werker, Janet F.},
