Title = {Individual differences in visual word recognition: Insights from the English Lexicon Project},
URL = {melvin@nus.edu.sg},
Volume = {38},
Year = {2012},
}


@article{2010-03463-00120100201,
Abstract = {I am very pleased to begin my term as the editor of Behavior Research Methods. As I write this editorial, I am looking at the table of contents for volume 41, number 4, which includes articles on lexical databases, design issues for fMRI studies, calculations of statistical power, and the development of MATLAB toolboxes for various purposes. The first issue of that year contained 179 pages, and that was pretty typical. Starting in 2007, the size of the issues increased dramatically. The last issue of 2009 contained 302 pages, and the current issue is close to 400. I would like to see the journal get closer to 200 pages per issue. This challenge is actually a great opportunity for the journal to remake itself. We will become more selective over the next few years, which should increase the overall quality of the journal. The strongest articles for Behavior Research Methods need to clearly define a need and then identify a solution that addresses that need. They both have diverse backgrounds that help us cover the wide range of topics submitted to the journal. We have been handling new submissions for the past year, and we look forward to your submissions in the future. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
