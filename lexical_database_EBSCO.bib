Year = {1984},
}


@article{1986-05458-00119841201,
Abstract = {Conducted 2 experiments during which 16 Japanese learners at an Australian university and 24 native Japanese readers made lexical classification to Hiragana or Katakana written Japanese words and foreign loan words. All Ss were undergraduate students. Results show a Stroop-like effect in the foreign loan word condition but not in the Japanese word condition among foreign Japanese learners, whereas a Stroop-like effect was shown in both Japanese and foreign loan word conditions among native Japanese readers. Based on these results, lexical representations of the native Japanese readers and foreign Japanese learners for the foreign loan words are discussed. Loan words refer to directly borrowed foreign words, such as the Japanese RA-JI-O for 'ratio.' 150 nonwords, half written in Hiragana and half in Katakana, were used as the stimuli for lexical decision. (6 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
