}


@article{2002-10549-00420010901,
Abstract = {A number of models of stuttering suggest that a problem with lexical retrieval is fundamental to the disorder. Such models predict, then, that lexical retrieval is a necessary condition for stuttering to occur. To investigate this prediction, this study looked at the effects of having persons who stutter speak in a way that does not involve lexical retrieval. If stuttering is a lexical retrieval problem, then stuttering should not occur under such conditions. In this study, 3 adults (aged 22-46 yrs) who stutter read aloud a standard English passage and two passages consisting of non-words, which were matched for length and initial sound to the words in the English passage. Stuttering occurred in all Ss at significant levels during the reading of the non-word passages. This finding is inconsistent with the idea that stuttering is caused by a problem with lexical retrieval. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Packman, Ann and  Onslow, Mark and  Coombes, Tanya and  Goodwin, Angela},
