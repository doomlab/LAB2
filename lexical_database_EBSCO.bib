Keywords = {grammatically anomalous sentences, anaphoric agreement violation, ERP analysis, subject-verb number agreement error, subject noun, Adolescent, Adult, Data Interpretation, Statistical, Electroencephalography, Evoked Potentials, Female, Humans, Language, Male, Psycholinguistics, Evoked Potentials, Grammar, Nouns, Sentences, Verbs, Sentence Structure},
