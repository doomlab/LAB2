Keywords = {aphasia, semantic memory, semantics, venlafaxine, vocabulary, Aphasia, Semantic Memory, Vocabulary, Semantics, Venlafaxine},
Number = {9},
Pages = {1030 - 1052},
Title = {Learning and maintaining new vocabulary in persons with aphasia: Two controlled case studies},
URL = {ltuomira@abo.fi},
Volume = {25},
Year = {2011},
