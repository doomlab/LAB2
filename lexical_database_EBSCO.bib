Journal = {Journal of Verbal Learning & Verbal Behavior},
Keywords = {auditory vs visual presentation of phonologically uniform word lists, modality effect within context of precategorical vs postcategorical hypotheses, college students, Auditory Perception, Human Information Storage, Phonology, Visual Perception, Words (Phonetic Units), Memory, Recall (Learning)},
