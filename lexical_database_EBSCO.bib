}


@article{2002-18587-00720020901,
Abstract = {The real-time lexical processing of children with specific language impairment (SLI) was examined, and the focus was to determine whether a temporal processing deficit or a general processing capacity deficit disrupts these children's lexical processing. A group of 21 SLI children (mean age 9), a group of 21 age-matched children (CA; mean age 8), and a group of 21 receptive syntax-matched children (RS; mean age 6) completed a sentence processing task in which they monitored sentences for a target word appearing at the beginning, middle, or end of sentences. Sentences systematically varied with respect to containing either a high proportion of stop consonants or a high proportion of nonstop consonants. Children also completed the Identification (ID) Speech Perception Task. On the sentence processing task, SLI children yielded significantly slower word recognition reaction time (RT) overall than both CA and RS controls. Sentence type had no effect on any subject group's RT. On the ID task, SLI children performed significantly worse than CA controls but similarly to RS children. The findings are interpreted to suggest that a general processing capacity limitation was responsible for the poor ID task performance and slower lexical processing of SLI children. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Montgomery, James W.},
ISSN = {0142-7164, 1469-1817},
Journal = {Applied Psycholinguistics},
Keywords = {lexical processing, specific language impairment, processing capacity, word recognition, sentence processing, temporal processing, reaction time, Cognitive Processing Speed, Language Disorders, Lexical Access, Sentence Comprehension, Word Recognition, Linguistics, Morphology (Language), Reaction Time, Temporal Lobe, Specific Language Impairment},
Number = {3},
Pages = {447 - 470},
Title = {Examining the nature of lexical processing in children with specific language impairment: Temporal processing or processing capacity deficit?},
URL = {Jim_Montgomery@med.unc.edu},
Volume = {23},
Year = {2002},
}


@article{2002-15811-00920020901,
Abstract = {Conducted a study on 10 Williams syndrome (WS) individuals (3 males and 7 females, aged 5 yrs and 6 mo-21 yrs and 4 mo) in which gender agreement and gender assignment were assessed by means of 2 tasks: gender categorization and gender concord. Subjects (Ss) performed gender categorization to real nouns whose gender was regular or exceptional given their ending, and to invented nouns which were composed of nonword or word stems and real word-endings. The same material was used in the gender concord task in which Ss had to match the items with the appropriate form of the article and the adjective carrying gender agreement. In the gender categorization of real words, WS Ss were lower than the 29 controls (aged 4 yrs and 9 mo-6 yrs and 1 mo) but both groups demonstrated sensitivity to gender-ending regularities. The WS Ss produced more ending-consistent responses than the controls. WS relied on the gender clue provided by the ending even when the gender of the word evoked by the stem of the invented word was opposed to that evoked by the ending. In the second task, the WS Ss performed well although lower than the controls. WS Ss were not impaired in gender agreement but experienced difficulties in retrieving lexical instances. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
Author = {Monnery, Sandrine and  Seigneuric, Alix and  Zagar, Daniel and  Robichon, Fabrice},
