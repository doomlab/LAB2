}


@article{2001-18421-00220010101,
Abstract = {Examined language function in the case of a male patient (aged 73 yrs) exhibiting profound global amnesia. At the age of 27, the S underwent a bilateral medial temporal lobe resection for intractable epilepsy, which resulted in profound global amnesia. The S was administered lexical memory, morphology, and syntactic processing tasks, and was assessed longitudinally with the Wechsler–Bellevue tests and the Wechsler Adult Intelligence Scale--Revised (WAIS--R). Results show that the S performed normally on tests of lexical memory and grammatical function relative to controls. Longitudinal data gathered on 20 occasions during a 48-yr period indicated consistent cognitive performance across time, and provided no evidence of a lexical memory decrement. It is concluded that medial temporal lobe structures are not critical for retention and use of already acquired lexical information or for grammatical processing, but are required for acquisition of lexical information. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Kensinger, Elizabeth A. and  Ullman, Michael T. and  Corkin, Suzanne},
