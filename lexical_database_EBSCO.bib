@article{2001-11342-01220010701,
Abstract = {Eye fixations during reading were monitored to examine the relationship between individual differences in working memory capacity—assessed by the reading span task—and inferences about predictable events. Context sentences predicting likely events, or non-predicting control sentences, were presented. They were followed by continuation sentences in which a target word represented an event to be inferred (inferential word) or an unlikely event (non-predictable word). A main effect of reading span showed that high working memory capacity was related to shorter gaze durations across sentence regions. More specific findings involved an interaction between context, target, and reading span on late processing measures and regions. For high- but not for low-span readers, the predicting condition, relative to the control condition, facilitated reanalysis of the continuation sentence representing the inference concept. This effect was revealed by a reduction in regression-path reading time in the last region of the sentence, involving less time reading that region and fewer regressions from it. Results show that working memory facilitates elaborative inferences during reading, but that this occurs at late text-integration processes, rather than at early lexical-access processes. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Calvo, Manuel G.},
ISSN = {0965-8211, 1464-0686},
Journal = {Memory},
Keywords = {eye fixations during reading, elaborative inferences during reading, predictable events, reading span, working memory capacity, text integration, Cognition, Eye Movements, Humans, Memory, Psychological Tests, Reading, Software, Vocabulary, Eye Fixation, Inference, Reading Comprehension, Short Term Memory},
Number = {4-6},
Pages = {365 - 381},
Title = {Working memory and inferences: Evidence from eye fixations during reading},
URL = {mgcalvo@ull.es},
Volume = {9},
Year = {2001},
}


@article{2001-18424-00220010701,
Abstract = {The structure of the French personality lexicon was investigated. Self-ratings on the 388 most frequently used French personality-descriptive adjectives were obtained from 415 French-speaking people (mean age 23 yrs). The scree plot of eigenvalues indicated 6 large factors. In the varimax-rotated 6-factor solution, the 4 largest factors, in order of size, corresponded fairly closely to the Big Five dimensions of Agreeableness, Emotional Stability, Extraversion, and Conscientiousness. The 5th factor was similar to the Honesty dimension found in several other languages. The 6th factor was defined by Imagination-related terms, but not by Intellect-related terms. Solutions involving 1 to 5 factors were also investigated and correlations between the factors that emerged from these different solutions are presented. The results are discussed in relation to other lexical studies of personality structure. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Boies, Kathleen and  Lee, Kibeom and  Ashton, Michael C. and  Pascal, Sophie and  Nicol, Adelheid A. M.},
