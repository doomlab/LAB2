Journal = {Memory & Cognition},
Keywords = {sources of sentence constraint on lexical ambiguity resolution, undergraduates, Analysis of Variance, Bias (Epidemiology), Cognition, Humans, Language Tests, Reaction Time, Reading, Semantics, Sex Factors, Homonyms, Lexical Decision, Sentence Comprehension, Sentence Structure, Stimulus Ambiguity},
