@article{1995-09025-00119930101,
Abstract = {Describes a learning theory (LT) that uses error detection based on positive-only evidence to explain how 1st-language learners acquire vocabulary items, particularly verbs, with speed and apparent ease. The formal properties of lexical representations are detailed, and learning is characterized with regard to the acquisition of designated morphological elements. The basic assumptions of the LT are discussed, including the prediction that argument-structure-changing morphology will cause errors that will be detected so that there is evidence for the learner. Varying lexicalization patterns and the difficulties they present for learning are discussed, and the manner in which learning prepositions and derivational affixes will constrain the course of lexical learning is described. The learning criterion is specified in terms of the point at which there are no further detectable lexical errors. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Wilkins, Wendy K.},
ISSN = {1048-9223, 1532-7817},
