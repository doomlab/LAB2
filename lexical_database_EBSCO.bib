Keywords = {lexical expression, language delay, Maltese children, limited word production, impaired language development, risk factors, vocabulary growth, Adolescent, Child Language, Female, Humans, Infant, Language, Language Development, Language Development Disorders, Language Tests, Male, Malta, Vocabulary, Language Delay, Language Development, Language Disorders, Lexical Access, Word Processing, Childhood Development, Risk Factors, Vocabulary},
Number = {6-7},
Pages = {459 - 471},
Title = {Early lexical expression in typically developing Maltese children: Implications for the identification of language delay},
