Keywords = {orthographic neighborhood, lexical access, strategic processing, visual word recognition, Lexical Access, Lexical Decision, Orthography, Word Recognition, Strategies, Visual Memory},
Number = {3},
Pages = {275 - 288},
Title = {O efeito de vizinhança ortográfica no Português do Brasil: Acesso lexical ou processamento estratégico? = The effect of orthographic neighborhood in Brazilian Portuguese: Lexical access or strategic processing?},
