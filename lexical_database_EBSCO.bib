@masterthesis{2010-99100-47620090101,
Abstract = {Syntactic priming occurs when processing one grammatical structure is facilitated by a previously encountered structure (Bock, 1986; Pickering & Branigan, 1998). These effects have been widely observed in production, and have recently been observed in comprehension (Pickering & Traxler, 2004; Arai, van Gompel, & Scheepers, 2007; and others). Syntactic priming may reflect residual activation for the prime sentence syntactic structure (Pickering & Branigan, 1998), implicit learning of the studied sentence structure (Bock & Griffin, 2000), or both. Experiment 1 investigated which mechanism(s) produce syntactic priming effects in comprehension. Participants took part in five eye tracking sessions that each contained 24 reduced-relative prime-target pairs with the same initial participle (RR-Prime: The client wanted by the advertiser was worth a lot of money; RR-Target: The actress wanted by the director was hesitant to confirm). Total fixation times on the prime sentences decreased across sessions at the critical regions, suggesting participants implicitly learned the structure. However, total time data revealed additional priming effects at the critical regions of the target sentences, and the magnitude of this effect did not change over the five sessions. This finding suggests that residual activation for the structure (that is stored with the repeated verb) also produces syntactic priming effects. These results are best explained by a dual mechanism account (residual activation and implicit learning) of syntactic priming effects. Experiment 2 investigated the time course of the previously observed syntactic priming effects. If residual activation for the prime sentence structure that is stored with the initial verb is quick to decay, then priming is unlikely to occur when there are multiple intervening sentences between the primes and targets. In Experiment 2 (a & b), zero, one, or three intervening sentences appeared between reduced-relative primes and targets with the same initial verbs. Significant priming effects were observed in the total time measure at the critical by-phrase region of the target sentences for all three conditions. The magnitude of this effect did not vary across conditions, which may suggest that the residual activation mechanism does not decay across intervening sentences that have different lexical items and syntactic structures. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Tooley, Kristen Michelle},
Keywords = {syntactic priming, sentence comprehension, time course, Priming, Sentence Comprehension, Syntax, Time, Implicit Learning, Sentences},
School = {ProQuest Information & Learning},
Title = {Syntactic priming in sentence comprehension: Mechanisms and time course},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2010-99100-476&site=ehost-live&scope=site},
Year = {2009},
}


@masterthesis{2009-99230-59420090101,
Abstract = {One of the main challenges in the study of cognition is how to connect brain activity to cognitive processes. In the domain of language, this requires coordination between two different lines of research: theoretical models of linguistic knowledge and language processing on the one side and brain sciences on the other. The work reported in this dissertation attempts to link these two lines of research by focusing on one particular aspect of linguistic processing, namely lexical access. The rationale for this focus is that access to the lexicon is a mandatory step in any theory of linguistic computation, and therefore findings about lexical access procedures have consequences for language processing models in general. Moreover, in the domain of brain electrophysiology, past research on event-related brain potentials (ERPs) - electrophysiological responses taken to reflect processing of certain specific kinds of stimuli or specific cognitive processes - has uncovered different ERPs that have been connected to linguistic stimuli and processes. One particular ERP, peaking at around 400 ms post-stimulus onset (N400) has been linked to lexico-semantic processing, but its precise functional interpretation remains controversial: The N400 has been proposed to reflect lexical access procedures as well as higher order semantic/pragmatic processing. In a series of three MEG experiments, we show that access to the lexicon from print occurs much earlier than previously thought, at around 200 ms, but more research is needed before the same conclusion can be reached about lexical access based on auditory or sign language input. The cognitive activity indexed by the N400 and its MEG analogue is argued to constitute predictive processing that integrates information from linguistic and non-linguistic sources at a later, post-lexical stage. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Almeida, Diogo},
Keywords = {MEG analogue, meaning, lexical access, behavioral evidence, cognitive processes, Cognitive Processes, Lexical Access, Meaning, Cognitive Science},
School = {ProQuest Information & Learning},
Title = {Form, meaning and context in lexical access: MEG and behavioral evidence},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2009-99230-594&site=ehost-live&scope=site},
Year = {2009},
}


@masterthesis{2009-99180-15220090101,
Abstract = {Purpose. Young children with Down syndrome (DS) usually have special difficulty acquiring expressive vocabulary and grammar, and recent studies have indicated that at least from early school ages, their speech production skills also are weaker than can be predicted by their intelligence scores. During the babbling stages, however, children with DS have often been found to produce canonical syllables roughly within a typical timeframe. The first purpose of this dissertation is to determine whether early phonological development in children with DS can best be characterized as delayed but predictable based on mental age, as is the case for early comprehension of vocabulary and syntax, or whether, like vocabulary and grammatical production, speech production is more severely impaired than could be anticipated by the children’s cognitive abilities alone. The second purpose of this dissertation is to examine the extent to which early vocabulary delays are associated with deficits in speech sound production. Method. Measures of consonant inventory and syllable structure level were taken at two points from 50 of the children studied by Warren et al. (2008). Canonical and noncanonical words and utterances, and the initial- and final-consonant inventory were coded during two15-minute videotaped semi-structured conversational samples with a parent at two points in time, 18 months apart. Results. For all measures at Time 1 (age ∼25 months), the children with DS performed equally well or better than their peers without DS (NDS). The reverse was true for all measures at Time 2 (∼43 months). Consonant inventory size at Time 1 predicted lexical growth at Time 2 only for the children with DS. For the NDS group of children, 60% of whom began the study with fewer than 2 productive consonants, only Time 1 cognitive scores predicted later lexical growth. Conclusion. A clear relationship between slow phonological growth and slow lexical growth at the period of 'first word' acquisition was established for children with DS. The results of this study indicate that the phonological skills in young children with DS are delayed beyond the level predictable by mental age at the period of early lexical development. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Sokol, Shari Baron},
Keywords = {phonological development, toddlers, Down syndrome, mixed-etiology developmental delays, Comprehension, Down's Syndrome, Infant Vocalization, Language Development, Phonology, Auditory Stimulation, Consonants, Developmental Disabilities, Etiology},
School = {ProQuest Information & Learning},
Title = {Phonological development in toddlers with Down syndrome and mixed-etiology developmental delays},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2009-99180-152&site=ehost-live&scope=site},
Year = {2009},
}


