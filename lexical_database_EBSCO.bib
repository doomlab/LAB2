Keywords = {developing lexicon, lexical changing influences, phonological variables, children's fast mapping, word learning, Attention, Child, Preschool, Comprehension, Female, Humans, Language Development, Male, Phonetics, Semantics, Verbal Learning, Vocabulary, Child Attitudes, Phonology, Mental Lexicon, Learning},
