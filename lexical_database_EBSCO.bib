Journal = {Language and Cognitive Processes},
Keywords = {serial lexical access, SERIOL model, letter position encoding, letter perceptibility, parallel lexical access, orthography, Human Information Storage, Letters (Alphabet), Lexical Access, Orthography, Serial Position Effect},
Number = {6},
