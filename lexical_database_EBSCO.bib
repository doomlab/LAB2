Keywords = {age of acquisition effects in word naming & lexical decisions in adulthood, undergraduates, Age Differences, Language Development, Lexical Decision, Reaction Time, Word Recognition},
Number = {2},
Pages = {167 - 180},
Title = {Real age of acquisition effects in word naming and lexical decision},
Volume = {91},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2000-15706-008&site=ehost-live&scope=site},
Year = {2000},
}


@article{2000-08368-00520000501,
Abstract = {Three experiments looked at the processing of interlexical homographs by Dutch–English bilinguals. In Exp 1 we employed the translation recognition task to force the Ss to activate both language systems simultaneously. In this task, the processing of interlexical homographs was inhibited substantially compared to the processing of matched control words. In Experiments 2 and 3 we used the lexical decision task: In one condition we asked Ss to categorize letter strings as words or nonwords in Dutch; in a second condition we asked them to do so in English. The makeup of the stimulus set in Exp 2 permitted the Ss to ignore the instructions and to instantiate the task in a language-neutral form—that is, to categorize the letter strings as words in either Dutch or English. Under these circumstances, a small, frequency-dependent inhibitory effect for homographs was obtained, but only in condition Dutch. In Exp 3, Ss were forced in a language-specific processing mode by the inclusion of 'nonwords' that were in fact words in the non-target language. Large frequency-dependent inhibitory effects for homographs were now obtained in both language conditions. The combined results are interpreted as support for the view that bilingual lexical access is non-selective. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
Author = {de Groot, Annette M. B. and  Delmaar, Philip and  Lupker, Stephen J.},
ISSN = {0272-4987, 1464-0740},
Journal = {The Quarterly Journal of Experimental Psychology A: Human Experimental Psychology},
Keywords = {lexical access & decision & semantic memory organization & translation recognition during processing of interlexical homographs, college students with Dutch-English bilingual ability, Bilingualism, Homographs, Lexical Access, Semantic Memory, Lexical Decision},
Number = {2},
Pages = {397 - 428},
Title = {The processing of interlexical homographs in translation recognition and lexical decision: Support for non-selective access to bilingual memory},
Volume = {53A},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2000-08368-005&site=ehost-live&scope=site},
Year = {2000},
}


@article{2000-07796-00620000501,
Abstract = {In 3 picture–word interference experiments the authors examined some properties of the semantic interference (SI) effect in the picture–word interference paradigm. In Exps 1 and 2, the authors tested if the SI may be obtained when the distractor words are not part of the response set and when only one picture per semantic category is included in the experiment. In Exp 3, the impact of the distractor words as part of the response set on the magnitude of the SI effect was explored. Reliable SI effects were obtained in all 3 experiments and the magnitude of the effect did not vary as a function of whether or not distractor words are part of the response set. The authors suggest that these results are problematic for the selection mechanism in the WEAVER++ lexical access model. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
