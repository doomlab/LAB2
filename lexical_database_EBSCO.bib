Volume = {45},
Year = {2002},
}


@article{2002-17410-00120020801,
Abstract = {Investigates the influence of phonological neighborhoods on the accuracy of speech production in aphasia by examining errors produced in both spontaneous and structured speech tasks. 43 aphasic Ss (mean age 67.9 yrs) recruited from several rehabilitation hospital and an outpatient day center were tested. Characteristics of the phonological neighborhoods of spontaneously produced aphasic errors are compared to the neighborhood characteristics of correctly produced targets in a picture description task. Accuracy of picture naming is also examined with reference to the phonological neighborhood characteristics of the stimuli. Results show that frequency of occurrence and neighborhood density play a facilitative role in speech production, replicating findings from recent studies with normal subjects. It is argued that the results are most parsimoniously explained within an interactive activation framework of lexical access. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
