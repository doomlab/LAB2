Year = {1974},
}


@article{2007-09226-00519691201,
Abstract = {Two variables, one syntactic and one lexical, were investigated for their effect on the retention of spoken English sentences. Mean phrase structure depth (d), based on the Yngve syntactic model, and lexical density (LD) were varied in a 2 × 2 within-subjects design, with List adding a between-subjects variable. The main results were that recall probability of individually presented sentences was not related to d, but was inversely related to LD, the proportion of lexical, as opposed to grammatical, words in the sentence. These results were replicated with a third list. The fact that d was unrelated to retention casts further doubt on the general value of the Yngve hypothesis, at least as presently interpreted and approximated by the d measure. Alternative interpretations of the model are briefly discussed. Finally, explanation of the LD effect is discussed in terms of lexical uncertainty and underlying relations. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Perfetti, Charles A.},
ISSN = {0022-5371},
Journal = {Journal of Verbal Learning & Verbal Behavior},
Keywords = {lexical density, phrase structure, sentence retention, syntax, retention, Lexical Access, Phrases, Retention, Sentences, Syntax},
Number = {6},
Pages = {719 - 724},
