ISSN = {0090-502X, 1532-5946},
Journal = {Memory & Cognition},
Keywords = {Klein effect on & interference with color- vs item-naming resulting from competition between stimulus–response associations vs task sets, college students, Adult, Cognition, Color Perception, Cues, Female, Humans, Inhibition (Psychology), Linguistics, Male, Refractory Period, Psychological, Set (Psychology), Word Association Tests, Color, Interference (Learning), Naming, Stimulus Parameters, Words (Phonetic Units), Lexical Decision, Semantic Priming, Stroop Effect},
