Volume = {19},
Year = {2006},
}


@article{2007-08039-01020060101,
Abstract = {Reviews the books, Conference interpreting: A students' companion by Andrew Gillies (2001); Interpreting: From preparation to performance. Recipes for practitioners and teachers by Csilla Szabó (2003); and Interpreting: Techniques and exercises by James Nolan (2005). Andrew Gillies' slim volume is volume 2 in the 'Language and Communication' series. In his introduction, the author advises the reader that his primary purpose is to offer a compendium of exercises intended to help students make swift progress and stay motivated in their independent interpreting practice. Gillies believes that 'deconstructing' the process of interpreting and practicing the individual component skills before reintegrating them again is the most effective approach. However, he gives ample warning that the reader hoping to relate practice to theoretical or pedagogical principles will be disappointed. In combination with their class-work and other readings, this little book could certainly be a source of ideas to enhance their motivation. Csilla Szabó paints a rather glum picture of the profession. The first article in chapters are designed to provide the reader with some basic definitions, although some should be approached with caution or that sight translation is a summary of a document. The recipes themselves comprise the second section of the book. The User's Guide precedes a series of 38 exercises which are supposedly grouped around four themes. However, it would not be wise to leave students unsupervised with this cookbook in the kitchen. The most recent of the pedagogical publications reviewed here is Interpretation: Techniques and exercises by James Nolan. This book, no doubt a life's work, draws on his lengthy experience at this influential international organization at the very center of world affairs. Readers might be disconcerted by the heavy emphasis on translation to practice what is surely an oral performance art. In all, this book is no quick fix in the shape of an off-the-shelf syllabus. However, with some judicious selection and adaptation, the abundance of materials and ideas might still prove a substantial resource both for instructors to complement their own lesson plans and for students wanting to target the lexical level in their practice. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Harmer, Jacolyn},
ISSN = {1384-6647, 1569-982X},
Journal = {Interpreting},
Keywords = {conference interpreting, teaching methods, interpretation, Comprehension, Language, Teaching Methods},
Number = {2},
Pages = {247 - 253},
Title = {Review of Conference interpreting: A students' companion; Interpreting: From preparation to performance Recipes for practitioners and teachers; and Interpreting: Techniques and exercises},
Volume = {8},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2007-08039-010&site=ehost-live&scope=site},
Year = {2006},
}


@article{2006-21157-00720060101,
Abstract = {The purpose of this study was to investigate the influence of conceptual and perceptual properties of words on the speed and accuracy of lexical retrieval of children who do (CWS) and do not stutter (CWNS) during a picture-naming task. Participants consisted of 13 3-5-year-old CWS and the same number of CWNS. All participants had speech, language, and hearing development within normal limits, with the exception of stuttering for CWS. Both talker groups participated in a picture-naming task where they named, one at a time, computer-presented, black-on-white drawings of common age-appropriate objects. These pictures were named during four auditory priming conditions: (a) a neutral prime consisting of a tone, (b) a word prime physically related to the target word, (c) a word prime functionally related to the target word, and (d) a word prime categorically related to the target word. Speech reaction time (SRT) was measured from the offset of presentation of the picture target to the onset of participant's verbal speech response. Results indicated that CWS were slower than CWNS across priming conditions (i.e., neutral, physical, function, category) and that the speed of lexical retrieval of CWS was more influenced by functional than perceptual aspects of target pictures named. Findings were taken to suggest that CWS tend to organize lexical information functionally more so than physically and that this tendency may relate to difficulties establishing normally fluent speech and language. Educational objectives: The reader will learn about and be able to (1) communicate the relevance of examining lexical retrieval in relation to childhood stuttering and (2) describe the method of measuring speech reaction times of accurate and fluent responses during a picture-naming task as a means of assessing lexical retrieval skills. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Hartfield, Kia N. and  Conture, Edward G.},
