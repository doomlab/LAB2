@article{2001-14015-00620010101,
Abstract = {Verbs like 'begin' and 'enjoy' appear to semantically select a complement that expresses an activity or an event (R. Jackendoff 1997). When these verbs occur with a complement that does not directly denote either an activity or an event (e.g., '…began the book' or '…enjoyed the book'), the complement must be type-shifted from an object to an activity to conform to the verb's semantic restrictions. The authors report an experiment in which self-paced reading times were found to be longer for complements that required type-shifting than for complements that directly matched the semantic restrictions of the matrix verb. The sample population consisted of 51 college students. The results are argued to provide on-line evidence for a type of enriched lexical processing posited in recent lexical semantic research. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {McElree, Brian and  Traxler, Matthew J. and  Pickering, Martin J. and  Seely, Rachel E. and  Jackendoff, Ray},
