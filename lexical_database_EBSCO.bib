@inbook{2016-08554-00820150101,
Abstract = {The problem concerning the integration between gesture and word is not yet solved. However, one aspect of the mechanism reciprocally relating the two signals (gesture and word) should be considered. Some studies addressed the problem of how the production of each signal is modified by the production of the other signal, given that the two signals are integrated. These works suggest a relation between the two signals deeper than a synchronism and, specifically, a relation between amodal representations of the two signals. Conversely, other studies addressed the problem of whether the presentation of one signal (prime), either implicitly or explicitly, facilitated or interfered with comprehension of the other target. Integration and comprehension are two processes that can be independent of each other and this fact may explain the differences seen in cortical activation during them. Indeed, it is not guaranteed that the circuit involved in facilitation/inhibition is the same as that involved in the integration of the two signals. In other words, one unanswered question is whether integration is coupled with lexical decision/semantic judgment. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
Author = {Gentilucci, Maurizio and  De Marco, Doriana and  De Stefani, Elisa and  Ferrari, Pier Francesco and  Rizzolatti, Giacomo},
Booktitle = {New frontiers in mirror neurons research.},
ISBN = {978-0-19-968615-5},
Pages = {138 - 152},
Publisher = {Oxford University Press},
Title = {On gesture and speech},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2016-08554-008&site=ehost-live&scope=site},
Year = {2015},
}


@article{2016-07444-00820150101,
Abstract = {This paper describes a research program in neurosemiotics. The main idea is to propose the N400 component of the Event-Related Potential (ERP) as an « empirical referee » between two paradigms aiming at understanding meaning constitution. The first one is the classic functionalist paradigm of cognitive sciences that describes computations for lexical access. The second one gathers Saussure structuralism, Husserlian phenomenology and Thom and Petitot morphodynamics (phenomenology, morphodynamics and structuralism, PMS paradigm). PMS assumes the co-presence of signified and signifier (Saussure), and the various modalisations of verbal consciousness (Husserl) together within a dynamics model involving meaning attractors without lexical access. The N400 component is proposed to empirically challenge the PMS paradigm that can help solve some empirical abnormalities that cannot be explained by the cognitive paradigm of lexical access. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Vion-Dury, Jean and  Besson, Mireille and  Cermolacce, Michel and  Schön, Daniele and  Piotrowski, David},
ISSN = {0769-4113, 2273-9262},
Journal = {Intellectica},
Keywords = {Event-related potentials, EEG, N400, phenomenology, Husseri, structuralism, Saussure, lexical access, cognitive sciences, morphodynamics, Cognitive Science, Evoked Potentials, Linguistics, Neurosciences, Phenomenology, Meaning, Models},
Number = {2},
Pages = {123 - 157},
Title = {Neurophénoménologie du signe linguistique: Apport du modèle Phénoménologique Morphodynamique et Structuraliste (PMS) à la compréhension des mécanismes neuraux sous-tendant la donation de sens = Neurophenomenology of the linguistic sign: Contribution of the phenomenological morphodynamics and structuralist (PMS) model to understanding the neural mechanisms underlying the constitution of meaning},
URL = {Jean.VION-DURY@ap-hm.fr, mireille.besson@amu-univ.fr, Michel.CERMOLACCE@ap-hm.fr, daniele.schon@univ-amu.fr, david.piotrowski@ehess.fr},
