Journal = {Journal of Memory and Language},
Keywords = {normal vs inverted vs mirror image vs reversed spelling text structure, retention of specific information & gist & transfer of pattern analyzing skill & lexical information, college students, Reading, Retention, Text Structure, Transfer (Learning), Concepts},
Number = {1},
