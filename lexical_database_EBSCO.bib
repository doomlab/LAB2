Volume = {12},
Year = {2000},
}


@article{2000-15293-00520000401,
Abstract = {Examined task-induced effects during visual lexical processing in a prime-target semantic priming paradigm with a total of 60 Ss (mean age 23 yrs). This research focused on target expectancy effects by manipulating the proportion of semantically related and unrelated word pairs. In Exp 1, a lexical decision task was used and reaction times (RTs) and event-related brain potentials (ERPs) were obtained. In Exp 2, Ss silently read the stimuli, without any additional task demands, and ERPs were recorded. The RT and ERP results of Exp 1 demonstrate that an expectancy mechanism contributed to the priming effect when a high proportion of related word pairs was presented. The ERP results of Exp 2 show that in the absence of extraneous task requirements, an expectancy mechanism is not active. However, a standard ERP semantic priming effect was obtained in Exp 2. The combined results show that priming effects due to relatedness proportion are induced by task demands and are not a standard aspect of online lexical processing. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
