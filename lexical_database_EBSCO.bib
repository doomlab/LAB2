Title = {Identity and similarity factors in repetition blindness},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=1996-95013-387&site=ehost-live&scope=site},
Year = {1996},
}


@masterthesis{1995-95015-13719950801,
Abstract = {Fetal alcohol syndrome (FAS) is a devastating disorder that affects approximately 0.29-0.48/1000 live born children. It is characterized by a pattern of facial malformations, pre- and/or postnatal growth deficiency, and CNS dysfunction. The most devastating features of FAS are the behavioral disturbances resulting from the CNS dysfunction. Children with FAS have been described as hyperactive, perseverative, and having attentional difficulties and are often mentally retarded. Until recently, brain abnormalities associated with FAS have been viewed as global and non-specific and thus not related to any specific pattern of behavioral dysfunction. Recently, however, studies of children with FAS have documented a disproportionate decrease in the volume of the basal ganglia, a group of subcortical brain structures that are involved in motor and cognitive abilities. This dissertation outlines a neurocognitive test of a model of the effects of prenatal alcohol exposure based upon a framework of basal ganglia dysfunction. Fourteen children with FAS, 14 normal controls (NC) and 12 children with Down syndrome (DS) were assessed on six tasks including measures of motor programming, implicit and explicit memory, verbal fluency, and behavior. Experiments were chosen based on the literature on Huntington's disease, a neurological disorder with known neuropathology primarily in the basal ganglia. Results were analyzed by multivariate and univariate analyses of variance. Planned comparisons between the FAS and NC groups and FAS and DS groups were conducted using separate or pooled variance t-tests. When compared with the NC group, the children with FAS demonstrated deficits on measures of verbal fluency (p's <02) and behavioral ratings (p's <05) and there were trends towards deficits on motor programming, explicit memory, perseverative responses, and prism adaptation. In contrast no differences were found between the FAS group and the NC group on measures of lexical or (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
