Year = {1985},
}


@article{1986-05448-00119850201,
Abstract = {Two experiments studied individual differences among normal adults in performance on the Nelson-Denny reading test to cast light on the processes involved in reading. Exp I (51 Ss aged 18–60 yrs) correlated reading comprehension with performance on the Daneman and Carpenter working memory span test, vocabulary, lexical decision with both homophonous and nonhomophonous nonwords, and a letter matching task, based on both physical and name matching. Working memory span proved to be a significant predictor, as did lexical decision with nonhomophonous nonwords, letter name matching, and vocabulary; and a derived measure indicating degree of phonological coding in lexical decision was only weakly correlated with comprehension, while the physical name match difference on the letter matching task was uncorrelated with reading performance. A 2nd experiment (66 female and 41 male Ss aged 19–60 yrs) explored further the working memory span task, comparing it with a nonlinguistic span test devised by R. D. Case et al (see record [rid]1982-25522-001[/rid]) based on memory and counting. While the verbal working memory measure again correlated significantly with comprehension, the counting measure was much more weakly related, and as in Exp I, vocabulary also correlated significantly with comprehension. It is concluded that reading comprehension depends on a number of separable components including vocabulary, working memory, and a general lexical access process. (17 ref) (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Baddeley, Alan and  Logie, Robert and  Nimmo-Smith, Ian},
ISSN = {0749-596X, 1096-0821},
