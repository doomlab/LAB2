Title = {Bilingual lexical activation in sentence context},
URL = {aischwartz@utep.edu, jfk7@psu.edu},
Volume = {55},
Year = {2006},
}


@article{2008-03552-00120060601,
Abstract = {Auxiliary verbs are among the most frequent words in language. The auxiliary selection is the process by which a verb selects its appropriate auxiliary in a specific language. In French language, this process is conducted when a verb is used with a compound tense form: Two auxiliaries are possible 'être' (be) and 'avoir' (have). Three types of linguistic information can influence the auxiliary selection: syntactic, lexical and aspectual. This study provided data about auxiliary selection of four aphasic patients, and four education- and age-matched normal subjects. The aphasic patients showed specific impairment of syntactic comprehension. In the present paper, the on-line sensitivity to the three constraints on auxiliary choice was tested in a word-monitoring task. The results of control normal subjects confirmed that this task showed an immediate sensitivity for all constraints. Consistent with their syntactic comprehension disorder, all aphasic patients were not sensitive to an auxiliary error when a syntactic constraint was violated. However, three aphasic patients remained sensitive to at least one auxiliary error. Two patients remained only sensitive to an auxiliary error in aspectual condition and a third patient was only sensitive to an auxiliary error in lexical condition. This finding suggests that aphasic patients can show an on-line sensitivity for some auxiliary errors. Our data are in favor of a partial independence between the three constraints. In particular, the aspectual constraint was preserved in patients who were not sensitive to lexical or syntactic constraints. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Griffini, Sophie and  Rigalleau, François},
ISSN = {1155-4452},
Journal = {Revue de Neuropsychologie},
Keywords = {real-time oral comprehension, auxiliary selection, aphasic individuals, sentence comprehension, syntactic comprehension, linguistics, lexical access, verbs, Aphasia, Linguistics, Lexical Access, Sentence Comprehension, Syntax, Verbs},
Number = {2},
Pages = {117 - 141},
Title = {La compréhension orale en temps réel des auxiliaires chez les sujets aphasiques = Real-time oral comprehension of auxiliaries in aphasic individuals},
URL = {sophie.griffini@mshs.univ-poitiers.fr},
Volume = {16},
Year = {2006},
}


@article{2006-13264-00420060601,
Abstract = {The present study analyzes the development of definitional skills in children of different age-levels and the role that metalinguistic awareness can act in the production of formally good definitions. Eighty-three children, divided into 4 age-groups (kindergartners, second graders, fourth graders, sixth graders), participated in the study and were administered two tasks: a definition task and a metalinguistic test. Results evidenced a clear age-related improvement in children's performances in both tasks. Moreover, the existence of a positive relation between metalinguistic awareness and definitional skills was confirmed and, in particular, the role of two specific components of metalinguistic competence (Concept of Definition and Lexical Awareness) emerged. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
