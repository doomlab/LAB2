Year = {1996},
}


@article{2017-27183-00119960101,
Abstract = {In this paper, we describe a new naming test and a set of detailed procedures that have been developed for selecting and coding responses. This is part of a larger collaborative study that seeks to explain normal and aphasic error patterns in terms of an interactive, spreading activation model of word retrieval. It is widely believed that error types seen in normal and aphasic naming are reflective of the stages of word retrieval. Semantic errors arise at the first stage, which involves retrieval of an abstract semantic-syntactic representation known as the lexical entry, or lemma. Phonemic paraphasias arise during later retrieval or assembly of the phonological form. Other errors appear to reflect the interaction of information across these two stages. One is the so-called mixed error: a substituted word that bears both semantic and phonological resemblance to the targe. Another is the formal paraphasia (malapropism; phonic verbal paraphasia): a word that bears a purely phonological resemblance to the target. On interactive, two-stage models, formal paraphasias reflect the influence of Stage 2 phonological information on Stage 1 lexical retrieval. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Roach, April and  Schwartz, Myrna F. and  Martin, Nadine and  Grewal, Rita S. and  Brecher, Adelyn},
ISSN = {0195-7015},
Journal = {Clinical Aphasiology},
Keywords = {Philadelphia Naming Test, aphasia, word retrieval, scoring, Aphasia, Lexical Access, Naming, Scoring (Testing)},
Pages = {121 - 133},
Title = {The Philadelphia Naming Test: Scoring and rationale},
Volume = {24},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2017-27183-001&site=ehost-live&scope=site},
