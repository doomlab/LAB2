Year = {1988},
}


@article{1989-18615-00119880901,
Abstract = {Tested 33 children (aged 9–11.5 yrs) divided into 3 groups (good readers and spellers, good readers and poor spellers, and poor readers and spellers). Ss performed a lexical decision task, deciding whether each letter-string was a word or not. Response times to 3 types of words (standard regular, common orthography but irregular spelling-to-sound relationships, and unusual orthography and irregular spelling-to-sound relationships) were compared. The performance of good readers but not of poor readers was impaired on the words that were phonologically irregular (compared with regular words). Results suggest that the regularity effect is phonologically and not orthographically mediated and that good readers use a predominantly phonological strategy in lexical decision. (French abstract) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
