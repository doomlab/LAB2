Keywords = {neuronal spoken word recognition, time course, speech recognition, event related potentials, Evoked Potentials, Neurons, Speech Perception, Word Recognition, Time},
Number = {2},
Pages = {159 - 183},
Title = {Neuronal spoken word recognition: The time course of processing variation in the speech signal},
URL = {Ulrike.schild@uni-hamburg.de},
Volume = {27},
Year = {2012},
}


@article{2012-08660-00920120201,
Abstract = {This article begins with a brief review of studies that have shown 1) an implicit learning, often as early as in the first grade, of graphotactic regularities and 2) the use of this kind of knowledge by elementary school children and adults while a priori more relevant rules, especially morphological rules, can be used. Then, we report a series of experiments that have investigated the influence of sensitivity to graphotactic regularities on the learning of word-specific spelling. These experiments suggest that the concordance between the orthographic structure of words and graphotactic regularities would cause (in part) the difficulties of acquisition of lexical knowledge. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
