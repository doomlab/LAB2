ISSN = {10705325},
Journal = {Numerical Linear Algebra with Applications},
Keywords = {INFORMATION retrieval, INFORMATION resources management, ABSTRACT algebra, DATABASES, DATABASE management, information, latent semantic indexing, low-rank, matrices, metrieval, orthogonal, singular value decomposition, sparse, ULV and URV decompositions, updating},
Number = {4},
