Journal = {Child Development},
Keywords = {semantic development, bilingual children, aging effects, language experience, Age Factors, Child, Child Language, Colorado, Female, Humans, Male, Multilingualism, Semantics, Texas, Bilingualism, Childhood Development, Language Development, Semantics, Age Differences},
Number = {3},
Pages = {1034 - 1045},
Title = {Semantic development in Spanish–English bilingual children: Effects of age and language experience},
URL = {li.sheng@mail.utexas.edu},
