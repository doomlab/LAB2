}


@article{1989-28344-00119890201,
Abstract = {Two experiments assessed the contributions of general and specific perceptual and conceptual information in the rereading of text passages printed in geometrically transformed typographies. P. A. Kolers (see record [rid]1976-29212-001[/rid]) showed that passages read 13–25 mo previously in transformed typography were reread faster than new passages. While Kolers attributed this facilitation to the retention of specific pattern-analyzing operations, it is also possible that the facilitation was associated with the retention of conceptual or lexical information. Using a total of 46 adults (aged 18–32 yrs), the present experiments found no evidence for the retention of specific perceptual information after one week, although there was strong evidence for the retention of gist and for transfer of both a general pattern-analyzing skill and of specific lexical information. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Tardif, Twila and  Craik, Fergus I.},
