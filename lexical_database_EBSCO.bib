Keywords = {semantic context, naming, pictures & words, lexical retrieval, German language, Analysis of Variance, Germany, Humans, Psycholinguistics, Reaction Time, Reading, Semantics, Speech, Visual Perception, Naming, Semantics, Words (Phonetic Units)},
