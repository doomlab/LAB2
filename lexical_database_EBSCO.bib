Title = {Effects of acoustic degradtion on syntactic processing: Implications for the nature of the sresource system used in language processing},
URL = {sashay@bu.edu},
Volume = {48},
Year = {2002},
}


@article{2002-12819-02720020301,
Abstract = {Investigated integration of the appropriate meaning of an ambiguous word into context by 10 nonfluent aphasic patients and 18 elderly control Ss. In the semantic priming experiment, ambiguous words (homonyms) were presented in different biasing sentence contexts at both a 0- and 750-msec interstimulus interval (ISI). Results show that at both ISIs, control Ss showed context-selective access, in that only contextually appropriate meanings were activated. In contrast, at the 0-msec ISI, the nonfluent aphasic patients showed activation of both meanings of the ambiguous word regardless of context. Only at the 750-msec ISI did they exhibit context-selective access. These results are consistent with the proposal that left hemisphere damage causing nonfluent aphasia results in an impairment in rapidly integrating lexical-semantic information into context. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
