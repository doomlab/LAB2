ISSN = {0265-5322, 1477-0946},
Journal = {Language Testing},
Keywords = {framework for development & evaluation & validation of vocabulary tests based on analysis of test purpose & test design & construct definition, Measurement, Test Construction, Test Validity, Vocabulary},
