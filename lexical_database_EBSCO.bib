Volume = {28},
Year = {2000},
}


@article{2000-16102-00220001001,
Abstract = {Examined the phonological disruption and subsequent self-correcting behavior in Cantonese aphasic speakers. The self-correcting behavior was investigated by examining the sequence of successive spontaneous attempts in producing a target sound. Five anomic, 5 non-fluent aphasic and 5 normal control Ss (aged 52–79 yrs) were assessed by using a confrontation naming task which included monosyllabic, disyllabic and trisyllabic targets. All the aphasic Ss demonstrated successive phonological self-corrections towards the target. Initial consonants were more vulnerable to phonological disruption and more resistant to self-correction than vowels, lexical tones and final consonants. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
