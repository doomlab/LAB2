Journal = {Journal of Experimental Psychology: Human Perception and Performance},
Keywords = {boundary technique, eye movements during reading, implicit lexical decision, word skipping, word recognition, oculomotor control, Eye Movements, Lexical Decision, Reading, Visual Tracking, Word Recognition, Perceptual Motor Processes, Priming},
Number = {5},
