Title = {Characterizing semantic space: Neighborhood effects in word recognition},
URL = {buchanan@uwindsor.ca},
Volume = {8},
Year = {2001},
}


@article{2001-11450-00220010801,
Abstract = {Examined the neurological processes underlying sentence comprehension in 2nd-language (L2) learners. event related potential (ERP) data were recorded as 12 native Japanese speakers (aged 21–33 yrs) who had learned German as a 2nd language after puberty made judgments concerning heard German sentences that were correct, semantically incorrect, syntactically incorrect, or both semantically and syntactically incorrect. Results show that overall performance was not perfect but clearly above chance. Compared to native controls, L2 Ss exhibited similar ERP patterns concerning semantically incorrect sentences. Correct sentences elicited a greater positivity in L2 Ss than native controls, possibly reflecting greater difficulties in syntactic integration. For sentences containing a phrase structure violation, L2 Ss did not show significant modulations of the syntax-related ERP components usually seen in native Ss. Sentences containing a pure semantic or a combined syntactic and semantic violation elicited a late right anterior–central negativity, an effect not observed for native language processing. It is concluded that the additional processes observed in 2nd-language learners are based on conceptual–semantic rather than on lexical–semantic aspects. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Hahne, Anja and  Friederici, Angela D.},
ISSN = {1366-7289, 1469-1841},
Journal = {Bilingualism: Language and Cognition},
Keywords = {neurological processes, sentence comprehension, second languages, event related potentials, semantics, syntax, Evoked Potentials, Foreign Language Learning, Neurolinguistics, Sentence Comprehension, Semantics, Syntax},
Number = {2},
Pages = {123 - 141},
Title = {Processing a second language: Late learners' comprehension mechanisms as revealed by event-related brain potential},
Volume = {4},
URL = {http://search.ebscohost.com.proxy.missouristate.edu/login.aspx?direct=true&db=psyh&AN=2001-11450-002&site=ehost-live&scope=site},
Year = {2001},
}


@article{2001-11200-00820010801,
Abstract = {Examined the role of neurolinguistic factors in stuttering by determining whether individuals who stutter display atypical neural functions for language processing, even with no speech production demands. Event-related brain potentials (ERPs) were obtained while 9 individuals who stutter (IWS) and 9 fluent speakers read sentences silently. The ERPs were elicited by: (a) closed-class words that provide structural or grammatical information, (b) open-class words that convey referential meaning, and (c) semantic anomalies. In standardized tests, IWS displayed similar grammatical and lexical abilities in both comprehension and production tasks compared to controls. Yet the ERPs elicited in IWS for linguistic processing tasks revealed differences in functional brain organization. The ERPs elicited in IWS were characterized by reduced negative amplitudes for closed-class words, open-class words, and semantic anomalies in a temporal window of approximately 200–400 ms after word onsets. Results indicate that alterations in processing for IWS are related to neural functions that are common to word classes and perhaps involve shared, underlying processes for lexical access. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
Author = {Weber-Fox, Christine},
ISSN = {1092-4388, 1558-9102},
Journal = {Journal of Speech, Language, and Hearing Research},
