Year = {1985},
}


@article{1986-05460-00119850201,
Abstract = {Examined K. I. Forster and S. M. Chambers's (see record [rid]1974-26697-001[/rid]) proposal that making lexical decisions to words and naming words access the same representations of those words in the mental lexicon. Five experiments with 74 college students revealed that the correlation between stimuli over tasks was due to word length in letters rather than to word frequency. In the experiment with orthographically illegal nonwords and orthographically legal but unwordlike nonwords, results show that lexical decisions were not influenced by word length but were influenced by word frequency. The letter length effect was due to a postaccess spelling check; and blocked naming, of words only, produced results similar to lexical decision, without length effects but with frequency effects indicative of lexical access. It is concluded that 2 separate and independent routes to word naming exist, both of which are mediated by lexical knowledge. (43 ref) (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
